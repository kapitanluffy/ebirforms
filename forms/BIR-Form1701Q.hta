            var v36a = NumWithComma(d.getElementById("frm1701q:txt36A").value) * 1;
            var total = 0.0;
            if (v36a <= 0) {
                total = 0;
            } else if (v36a < 10000) {
                total = (v36a * (5 / 100)).toFixed(2);
            } else if (v36a >= 10000 && v36a < 30000) {
                total = ((v36a - 10000) * (10 / 100) + 500).toFixed(2);
            } else if (v36a >= 30000 && v36a < 70000) {
                total = ((v36a - 30000) * (15 / 100) + 2500).toFixed(2);
            } else if (v36a >= 70000 && v36a < 140000) {
                total = ((v36a - 70000) * (20 / 100) + 8500).toFixed(2);
            } else if (v36a >= 140000 && v36a < 250000) {
                total = ((v36a - 140000) * (25 / 100) + 22500).toFixed(2);
            } else if (v36a >= 250000 && v36a < 500000) {
                total = ((v36a - 250000) * (30 / 100) + 50000).toFixed(2);
            } else if (v36a >= 500000) {
                total = ((v36a - 500000) * (32 / 100) + 125000).toFixed(2);
            }

            //d.getElementById("frm1701q:txt37A").value = (d.getElementById("frm1701q:txt36A").value*1 * 5/100).toFixed(2);
            d.getElementById("frm1701q:txt37A").value = formatCurrency(total);
