function validateItem2() {
	var dt = new Date();
	
	if (d.getElementById('frm2551:txtYear').value == "") {
        alert("Please indicate a valid Year.");
        return;
    }
	if (d.getElementById('frm2551:rtnMonth').value == 0) {
        alert("Please choose a valid Month.");
        return;
    }
    if (d.getElementById('frm2551:txtYear').value*1 > dt.getFullYear()*1) {
        alert("Invalid date entry on Item 2. Entry should not be later than Current Date.")
        return;
    }
	if (d.getElementById('frm2551:txtYear').value*1 == dt.getFullYear()*1 && d.getElementById('frm2551:rtnMonth').value-1 > dt.getMonth()) {
		alert("Invalid date entry on Item 2. Entry should not be later than Current Date.")
        return;
	}
}

