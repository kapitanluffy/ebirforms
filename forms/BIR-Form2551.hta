    function getATCCode()
    {
        var x = 1;
        var isOptChecked;
        while(x < codeATC.length){
            isOptChecked = d.getElementById('frm2551:optATC' + x).checked;
            if(isOptChecked){
                if(compareATC(codeATC[x])){
                    return;
                }else {
                    if(rowFlag == "14"){
                        d.getElementById('frm2551:txtTax14A').value = descATC[x];
                        d.getElementById('frm2551:txtTax14B').value = codeATC[x];
                        d.getElementById('frm2551:txtTax14D').value = rateATC[x];
                        computeTaxDue('frm2551:txtTax14C', 'frm2551:txtTax14D', 'frm2551:txtTax14E');
                    } else if(rowFlag == "15"){
                        d.getElementById('frm2551:txtTax15A').value = descATC[x];
                        d.getElementById('frm2551:txtTax15B').value = codeATC[x];
                        d.getElementById('frm2551:txtTax15D').value = rateATC[x];
                        computeTaxDue('frm2551:txtTax15C', 'frm2551:txtTax15D', 'frm2551:txtTax15E');
                    } else if(rowFlag == "16"){
                        d.getElementById('frm2551:txtTax16A').value = descATC[x];
                        d.getElementById('frm2551:txtTax16B').value = codeATC[x];
                        d.getElementById('frm2551:txtTax16D').value = rateATC[x];
                        computeTaxDue('frm2551:txtTax16C', 'frm2551:txtTax16D', 'frm2551:txtTax16E');
                    } else if(rowFlag == "17"){
                        d.getElementById('frm2551:txtTax17A').value = descATC[x];
                        d.getElementById('frm2551:txtTax17B').value = codeATC[x];
                        d.getElementById('frm2551:txtTax17D').value = rateATC[x];
                        computeTaxDue('frm2551:txtTax17C', 'frm2551:txtTax17D', 'frm2551:txtTax17E');
                    } else if(rowFlag == "18"){
                        d.getElementById('frm2551:txtTax18A').value = descATC[x];
                        d.getElementById('frm2551:txtTax18B').value = codeATC[x];
                        d.getElementById('frm2551:txtTax18D').value = rateATC[x];
                        computeTaxDue('frm2551:txtTax18C', 'frm2551:txtTax18D', 'frm2551:txtTax18E');
                    }

                    clearATCFlag();
                }
            }
            x++;
        }

        hideModalATC();
    }

    function compareATC(atc)
    {
        var y = 0;
        while(y < atcFlag.length){
            if(atcFlag[y] == atc){
                alert("Invalid input. Selected ATC already defined.");
                return true;
            }
            y++;
        }
        return false;
    }

    function clearATCFlag()
    {
        atcFlag = new Array();
        var x = 14;
        while(x < 19){
            if(d.getElementById('frm2551:txtTax' + x + 'B').value != ""){
                atcFlag[atcFlag.length] = d.getElementById('frm2551:txtTax' + x + 'B').value;
            }

            x++;
        }
    }

    function checkOverpayment()
    {
        if(NumWithComma(d.getElementById('frm2551:txtTax23').value) < 0){
            d.getElementById('frm2551:opt23_1').disabled = false;
            d.getElementById('frm2551:opt23_2').disabled = false;
