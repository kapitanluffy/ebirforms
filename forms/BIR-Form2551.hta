            d.getElementById('frm2551:opt23_2').disabled = true;
            d.getElementById("frm2551:opt23_1").checked = false;
            d.getElementById("frm2551:opt23_2").checked = false;
			
        }
    }
	
	function computeTaxDue(taxAmount, taxRate, taxDue)
    {
            d.getElementById(taxDue).value = formatCurrency(((NumWithComma(d.getElementById(taxAmount).value)*1) / 100) * (NumWithComma(d.getElementById(taxRate).value)*1));

            computeTotalTaxDue();
    }

    function computeTotalTaxDue()
    {
        var total = 0;
        var x = 14;
        while(x < 19){
            total = (total*1) + (NumWithComma(d.getElementById('frm2551:txtTax' + x + 'E').value)*1);
            x++;
        }
        d.getElementById('frm2551:txtTax19').value = formatCurrency(total*1);

        computeTaxPayable();
    }

    function computeTotalTaxCreditForm2551()
    {
        d.getElementById('frm2551:txtTax20C').value = formatCurrency((NumWithComma(d.getElementById('frm2551:txtTax20A').value)*1) + (NumWithComma(d.getElementById('frm2551:txtTax20B').value)*1));

        computeTaxPayable();
    }

    function computeTaxPayable()
    {
        d.getElementById('frm2551:txtTax21').value = formatCurrency((NumWithComma(d.getElementById('frm2551:txtTax19').value)*1) - (NumWithComma(d.getElementById('frm2551:txtTax20C').value)*1));

        computeTotalAmountPayable();
    }
