function loadBGData() {
    /*This will load data onto Background Information fields*/
    var responseBG = d.getElementById("responseBG");

    var formTypeVal = String(responseBG.innerHTML).split('formType='); //form_type

    if (formTypeVal[1] == '0619F') {

        var fnVal = String(responseBG.innerHTML).split('fn=');
        d.getElementById('frm0619F:txtTaxpayerName').value = unescape(fnVal[1]);

        var tin1Val = String(responseBG.innerHTML).split('tin1=');
        d.getElementById('frm0619F:txtTIN1').value = 	tin1Val[1];

        var tin2Val = String(responseBG.innerHTML).split('tin2=');
        d.getElementById('frm0619F:txtTIN2').value = 	tin2Val[1];

        var tin3Val = String(responseBG.innerHTML).split('tin3=');
        d.getElementById('frm0619F:txtTIN3').value = 	tin3Val[1];

        var tin4Val = String(responseBG.innerHTML).split('tin4=');
        d.getElementById('frm0619F:txtBranchCode').value = tin4Val[1];

        var rdoVal = String(responseBG.innerHTML).split('rdo=');
        d.getElementById('frm0619F:txtRDOCode').value = rdoVal[1];

        var lobVal = String(responseBG.innerHTML).split('lob=');
        d.getElementById('frm0619F:txtLineBus').value = lobVal[1];

        //address to split when exceeds to 127 character.
        var regAddrVal = String(responseBG.innerHTML).split('regAddr=');
        var address = regAddrVal[1];
        var addUpper = address.toUpperCase();	
        if (regAddrVal[1].length > 127) {
            d.getElementById('frm0619F:txtAddress').value = 	unescape(addUpper.substring(0, 127));
            d.getElementById('frm0619F:txtAddress2').value = 	unescape(addUpper.substring(127, addUpper.length));
        }
        else {
        d.getElementById('frm0619F:txtAddress').value = 	unescape(addUpper);	
        d.getElementById('frm0619F:txtAddress2').value = 	"";
        }

        var zipVal = String(responseBG.innerHTML).split('zip=');
        d.getElementById('frm0619F:txtZipCode').value = zipVal[1];

        var telNoVal = String(responseBG.innerHTML).split('telNo=');
        d.getElementById('frm0619F:txtTelNum').value = telNoVal[1];
    }

    var emailVal = String(responseBG.innerHTML).split('email=');

    if(emailVal[1] === undefined){
        document.getElementById('txtEmail').value = "";
    }
    else{
        document.getElementById('txtEmail').value = emailVal[1];
        globalEmail = emailVal[1];
    }
}

