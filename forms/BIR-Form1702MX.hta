                alert("There are no " + attachType + " attachments available.");

                if (totalEX === 0) {
                    disableAttachColumns(false, 'EX');

                    viewAttachments("SP", true);
                }
                else if (totalSP === 0) {
                    disableAttachColumns(false, 'SP');

                    viewAttachments("EX", true);
                }
            }
        }
