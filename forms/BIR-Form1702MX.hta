                    if (disabledColumns[i].value.length > 0) {
                        disabledColumns[i].value = "";

                        if (typeof (disabledColumns[i].onblur) === "function") {
                            disabledColumns[i].onblur();
                        }
                    }

                    disabledColumns[i].disabled = !!disable;
