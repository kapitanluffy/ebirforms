            }
            else if (!!isIC030or031) {
                if (someValue > 50) {
                    optionToSelect = 30;
                }
            }
            else if (!!isPEZABCDAorECOZONE5) {
                optionToSelect = 5;
                tempVarForSc1I12 = (optionToSelect === 5);
