            if (!!isSpecial && (totalSP > 0)) {
                document.getElementById('frm1702MX:' + array[i]).disabled = true;
            }
            else if (!!isExempt && (totalEX > 0)) {
                document.getElementById('frm1702MX:' + array[i]).disabled = true;
            }
            else {
                document.getElementById('frm1702MX:' + array[i]).disabled = false;
