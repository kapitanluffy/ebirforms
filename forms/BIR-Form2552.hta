        while(indexParam < tblTaxTrans.rows.length - 1){ //alert("indexParam " + indexParam + " tblsize " + tblTaxTrans.rows.length)
            chkBox = d.getElementById('chkTaxTrans' + ((indexParam*1) + 1));
