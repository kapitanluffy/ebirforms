function saveChangesPg5Sch1(modalID, bool){

	var tempTable = d.getElementById('tblGrossCompensationAndTaxWithheld');
	var mainTable = d.getElementById('tblGrossCompensationAndTaxWithheldRepository');
	
	//=============================================================================================
	//VALIDATE ROW
	//=============================================================================================
	var span = tempTable.getElementsByTagName('span');
	//store valid span here,, is span is format(number.number);
	var validSpan = [];
	var count = 0;
	var baseNumber = 0;
	//validate if span is format(number.number);
	for (var i = 0; i < (span.length); i++) {

		var iterationNumber = span[i].innerHTML.split('.');

		if (iterationNumber.length == 2) {

			if ((!isNaN(iterationNumber[0])) && (!isNaN(iterationNumber[1]))) {
				validSpan[count] = span[i];
				count++;
			}
		}
	};
	//get iteration number
	//to be used in error message during validation
	if (validSpan.length > 0) {
		baseNumber = validSpan[0].innerHTML.split('.')[0];
	}

	var isValid = validateModalPg5Sc1(tempTable.id, baseNumber);
	
	//=============================================================================================
	//IF SAVE 
	//=============================================================================================
	if ((bool == -1) || (bool === true)) {
		
		if (isValid) {
			
			//add alert // remove alert during reloading
			if (bool === true) { alert("Saved!"); }
			
			//=====================================================================================
			//NOT USED : USER CANNOT SAVE if theres an empty row
			//=====================================================================================
			/*
			//validate row, check if description != empty string
			var tr = tempTable.getElementsByTagName('tr');
			//in 1 full row,, there are 14 tr
			var i = tr.length
			while (i > 0) {
				i -= 14;

				var inputs = [];
				inputs = tr[i].getElementsByTagName("input");
				var id = [];
				id = inputs[0].id.split(":");

				if (($("#" + id[0] + "\\:" + id[1]).attr("type")) === "text") {

					if (inputs[0].value.toString().trim() === "") {

						deleteRowPage5Sched1(inputs[0]);
					}
				}
			}
			*/
			//=====================================================================================
			//set mainTable same as tempTable's content.
			copyModalPg5Sc1(1);

			//reset IDs
			resetMainTableIDPg5Sc1();
			
			isValidToClose = true;
			
			//=====================================================================================
			//check maintable rows after save
			//=====================================================================================
			//for reloading
			activeParentInput  = [];
			activeParentInput[0] = 'frm1701:txtPg5Sc1I4NameOfEmp';
			activeParentInput[1] = 'frm1701:txtPg5Sc1I4EmpTIN1C1';
			activeParentInput[2] = 'frm1701:txtPg5Sc1I4EmpTIN2C1';
			activeParentInput[3] = 'frm1701:txtPg5Sc1I4EmpTIN3C1';
			activeParentInput[4] = 'frm1701:txtPg5Sc1I4EmpTIN4C1';
			activeParentInput[5] = 'frm1701:txtPg5Sc1I4ComIncomeC2';
			activeParentInput[6] = 'frm1701:txtPg5Sc1I4TaxWithheldC3';
				
			if (mainTable.rows.length <=1){
			
				if (mainTable.rows.length === 0){
					//if zero, reset item 4
					disableItem4(false);
				}else{
					//transfer mainTable first row to mainPage Item 4
					transferToModalPg5Sc1(false);
					
					//delete item4 to disabledinputIDs[]
					for (var i = 0 ; i < activeParentInput.length ; i++){
					
						var deletedIndex = -1;
						
						for (var x = 0 ; x < disabledinputIDs.length ; x++){
							
							if (activeParentInput[i] == disabledinputIDs[x]){
								
								var id = activeParentInput[i].split(":");
								$("#"+id[0]+"\\:"+id[1]).removeClass("disabledOthers");
								
								deletedIndex = x;
							}
						}
						if (deletedIndex != -1){
							disabledinputIDs.splice(deletedIndex, 1);
						}
					}
					storeDisabledinputIDs();
				}
			}else{
			
				disableItem4(true);
				
				//setCounter if mainTable has more than 1 row
				var trMainTable = mainTable.getElementsByTagName('tr');
				d.getElementById("frm1701:txtCtr" + modalID).value = trMainTable.length / 14;
				
				//store item4 to disabledinputIDs[]
				for (var i = 0 ; i < activeParentInput.length ; i++){
					
					var doAdd = true;
					
					for (var x = 0 ; x < disabledinputIDs.length ; x++){
						
						if (activeParentInput[i] == disabledinputIDs[x]){
							doAdd = false;
							break;
						}
					}
					
					if (doAdd){

						disabledinputIDs.push(activeParentInput[i]);
					}
				}
				
				storeDisabledinputIDs();
			}
			
			//recompute on saveChangesPg5Sch1() if SAVED 
			sumIterateOnlyPg5Sc1(true);
			
		}else{
			
			isValidToClose = false;
		}
	//=============================================================================================
	//IF CANCEL
	//=============================================================================================
	}else if(bool === false) {
	
		alert("Disregard changes.");
		
		//mainTable to tempTable
		copyModalPg5Sc1(2);
		
		//recompute on saveChangesPg5Sch1() if CANCEL
		//if bool == false,,
		///if true, recompute is in closeModalPg5Sc1()
		sumIterateOnlyPg5Sc1(false);
	}
}

function resetMainTableIDPg5Sc1() {

	var idX = 5;
	var tr = d.getElementById('tblGrossCompensationAndTaxWithheldRepository').getElementsByTagName('tr');
	//in 1 full row,, there are 14 tr
	var x = 0;
	while (x < tr.length) {
	
		var inputs = [];
		inputs = tr[x].getElementsByTagName("input");
		
		for (var i=0; i<inputs.length; i++){

			if (inputs[i].type === "text"){
				
				var desc = "";
				
				if (inputs[i].id.indexOf('NameOfEmp') > -1){
					desc = "NameOfEmp";
				}else if (inputs[i].id.indexOf('EmpTIN1C1') > -1){
					desc = "EmpTIN1C1";
				}else if (inputs[i].id.indexOf('EmpTIN2C1') > -1){
					desc = "EmpTIN2C1";
				}else if (inputs[i].id.indexOf('EmpTIN3C1') > -1){
					desc = "EmpTIN3C1";
				}else if (inputs[i].id.indexOf('EmpTIN4C1') > -1){
					desc = "EmpTIN4C1";
				}else if (inputs[i].id.indexOf('ComIncomeC2') > -1){
					desc = "ComIncomeC2";
				}else if (inputs[i].id.indexOf('TaxWithheldC3') > -1){
					desc = "TaxWithheldC3";
				}
				
				//alert(inputs[i].id + " :: " + "frm1701:txtPg5Sc1I" + idX + desc + " :: "+inputs[i].maxLength);
				inputs[i].id = "frm1701:txtPg5Sc1I" + idX + desc;
			
			}else if (inputs[i].type === "radio"){
				
				var desc = "";
				
				if (inputs[i].id.indexOf('Taxpayer') > -1){
					desc = "Taxpayer";
					
				}else if (inputs[i].id.indexOf('Spouse') > -1){
					desc = "Spouse";
