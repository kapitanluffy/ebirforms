	if (!isPrintingFormPages){
		alert(landscapeMsg + "This form must be printed on an A4 Size Paper. Please update your Printer Settings to ensure the correct printout of the form.\n\n" +
			  "If applicable, open Internet Explorer, go to File -> Page Setup and change Page Size to Legal, all Margins must be 0.166 and all Headers & Footers must be set to empty.");
	}
	
	isPrintingFormPages = true;
} 

