    function nolcoDateValidation(e, elementName, taxYearId) {
        var years = d.getElementsByName(elementName);
        var sameCounter = 0;
        var today = new Date();
        var curYear = today.getFullYear();

        for (i = 0; i < years.length; i++) {
            if (e.value != "") {
                if (e.value == years[i].value) {
                    sameCounter++;
                }
            }
        }
        if (e.value > parseInt("20" +d.getElementById(taxYearId).value)) {
            alert("Year Incurred cannot be more than Tax Return Year in Page 1");
            e.value = "";
        }
        else if (sameCounter >= 2) {
            alert("Cannot have duplicate year for declared Year incurred");
            e.value = "";
        }
        else if (e.value != "" && e.value < (parseInt("20" + d.getElementById('frm1701:txtPg1I1Year').value) - 3)) {
            alert("Year Incurred Cannot be less than 3 years");
            e.value = "";
        }
    }

