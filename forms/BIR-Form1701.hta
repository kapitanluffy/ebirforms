    // Dynamic Summation function.
    function summation(containerFrom, from, to) {
        var toArray = to.split(',');
        var fromIDs = from.split(',');
        var containerAray = containerFrom.split(',');
        
        var summation = 0

        for (var a = 0; a < containerAray.length; a++) {
            var controls = d.getElementById(containerAray[a]).getElementsByTagName("input");

            for (var i = 0; i < controls.length; i++) {
                var controlID = controls[i].id;

                for (var b = 0; b < fromIDs.length; b++) {
                    var controlsElement = controlID.indexOf(fromIDs[b]);
                    if (controlsElement > -1) {

                        var controlValue = removeCommaParenthesis(controlID);

                        if (controlValue.toString().trim() === "") {
                            summation = summation + 0;
                        }
                        else {
                            summation = summation + convertToFloat(controlValue);
                        }
                        

                        for (var c = 0; c < toArray.length; c++) {
                            d.getElementById(toArray[c]).value = NegativeValue(formatCurrencyWOC(Math.round(summation)));
                        }
                    }
                }
            }
        }
    }

    function differenceOfElements(param, param2, outputControlID) {
        var paramValue = removeCommaParenthesis(param);
        var param2Value = removeCommaParenthesis(param2);

        var diff = convertToNumber(paramValue) - convertToNumber(param2Value);
        d.getElementById(outputControlID).value = NegativeValue(formatCurrencyWOC(Math.round(diff)));
    }

    function percentageOfElement(percentage, senderID, outputControlID) {
        var senderValue = removeCommaParenthesis(senderID);
        var result = 0;
        if (senderValue <= 0) {
            d.getElementById(outputControlID).value = NegativeValue(formatCurrencyWOC(result));
        }
        else {
            result = Math.round(parseFloat(senderValue) * parseFloat(percentage));
            d.getElementById(outputControlID).value = NegativeValue(formatCurrencyWOC(result));
        }
    }

    function removeCommaParenthesis(senderID) {
        var senderValue = d.getElementById(senderID).value;

        if (senderValue.toString().indexOf('(') != -1 || senderValue.toString().indexOf(')') != -1) {
            senderValue = NumWithParenthesis(senderValue);
        }

        if (senderValue.toString().indexOf(',') != -1) {
            senderValue = WholeNumWithComma(senderValue);
        }

        if(!isNaN(senderValue)){
            return (senderValue * 1);
        }
        else {
             return senderValue;
        }
    }

    function moveValue(senderID, outputControlID) {
        d.getElementById(outputControlID).value = d.getElementById(senderID).value;
    }

    function checkPercentage(percentage, senderID, compareID, errorMessage, mode) {
      
		percentage = convertToFloat(percentage / 100);
        var senderValue = removeCommaParenthesis(senderID);
        var compareValue = removeCommaParenthesis(compareID);

        var result = Math.round(convertToFloat(parseFloat(compareValue) * parseFloat(percentage)));

        if (convertToFloat(senderValue) > result && mode == "" && result > 0) {
            alert(errorMessage);
            d.getElementById(senderID).value = 0;
        }
        else if (mode == "return") {
            if (convertToFloat(senderValue) <= result || result < 0) {
                errorMessage = "";
            }
            return errorMessage;
        }
    }

