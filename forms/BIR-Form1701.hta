					if (radioID == "frm1701:rdoPg5Sc1I" + [a] + "Spouse") {
						if (d.getElementById("frm1701:rdoPg5Sc1I" + [a] + "Spouse").checked) {
							totalSpouse += parseInt(removeCommaParenthesis("frm1701:txtPg5Sc1I" + [a] + "ComIncomeC2"));
							totalTaxWithheldSpouse += parseInt(removeCommaParenthesis("frm1701:txtPg5Sc1I" + [a] + "TaxWithheldC3"));
						}
					}
				}
			}
		}
   } 

	d.getElementById("frm1701:txtPg5Sc1I5ATotalComIncome").value = NegativeValue(formatCurrencyWOC(totalTaxPayer));
	d.getElementById("frm1701:txtPg5Sc1I5BTotalComIncome").value = NegativeValue(formatCurrencyWOC(totalSpouse));
	d.getElementById("frm1701:txtPg5Sc1I5ATotalTaxWithheld").value = NegativeValue(formatCurrencyWOC(totalTaxWithheldTaxPayer));
	d.getElementById("frm1701:txtPg5Sc1I5BTotalTaxWithheld").value = NegativeValue(formatCurrencyWOC(totalTaxWithheldSpouse));

	moveValue('frm1701:txtPg5Sc1I5ATotalTaxWithheld', 'frm1701:txtPg9Sc9I5TaxPayerC1');
	moveValue('frm1701:txtPg5Sc1I5BTotalTaxWithheld', 'frm1701:txtPg9Sc9I5SpouseC2');
	summation('Page9Content', 'Sc9I1Spouse,Sc9I2Spouse,Sc9I3Spouse,Sc9I4Spouse,Sc9I5Spouse,Sc9I6Spouse,Sc9I7Spouse,Sc9I8Spouse,Sc9I9Spouse', 'frm1701:txtPg9Sc9I10SpouseC2');
	summation('Page9Content', 'Sc9I1Tax,Sc9I2Tax,Sc9I3Tax,Sc9I4Tax,Sc9I5Tax,Sc9I6Tax,Sc9I7Tax,Sc9I8Tax,Sc9I9Tax', 'frm1701:txtPg9Sc9I10TaxPayerC1');
