	for (var i = 0; i < elem.length; i++) {
	
		if (elem[i].type != 'hidden') { 
			
			if (elem[i].type == 'text') {
			
				if (d.getElementById(elem[i].id).disabled) {
					disabledItems[x] = elem[i].id;
					x++;
				}
				d.getElementById(elem[i].id).disabled = false;
				d.getElementById(elem[i].id).readOnly = true;
				d.getElementById(elem[i].id).style.backgroundColor = '#FFFFFF';
				d.getElementById(elem[i].id).style.color = '#000000';
				d.getElementById(elem[i].id).style.height = "9px";
				d.getElementById(elem[i].id).style.fontSize = "8px";
