function disableATCSchedules(schedules, lastKeyword, isDisable, isReset) {

	isReset = (typeof (isReset) === "undefined") ? true : isReset;
	var scheduleArray = schedules.split(',');

	for (var i = 0; i < scheduleArray.length; i++) {
	
		$("input[id*='" + scheduleArray[i] + "'][id$='" + lastKeyword + "'][name!='frm1701" + "\\:" + "ModalDesc'][name!='frm1701" + "\\:" + "TaxFiler']").attr("disabled", isDisable);
		
		if (isReset) {
		
			$("input[id*='" + scheduleArray[i] + "'][id$='" + lastKeyword + "'][name!='frm1701" + "\\:" + "ModalDesc'][name!='frm1701" + "\\:" + "TaxFiler']").val("");
			$("input[id*='" + scheduleArray[i] + "'][id$='" + lastKeyword + "'][maxlength='12'][name!='frm1701" + "\\:" + "TaxFiler']").val("0");
		}
	}
}

function addClassName(schedules, lastKeyword, person) {

	isReset = (typeof (isReset) === "undefined") ? true : isReset;
	var scheduleArray = schedules.split(',');

	for (var i = 0; i < scheduleArray.length; i++) {
	
		if (lastKeyword != '') {
			$("input[id*='" + scheduleArray[i] + "'][id$='" + lastKeyword + "'][name!='frm1701" + "\\:" + "ModalDesc'][name!='frm1701" + "\\:" + "TaxFiler']:disabled").addClass("disabledSchedules");
		} else {
			$("input[id*='" + scheduleArray[i] + "'][name!='frm1701" + "\\:" + "ModalDesc'][name!='frm1701" + "\\:" + "TaxFiler']:disabled").addClass("disabledSchedules");
		}

		if (person !== '') {
			$("input[id*='" + scheduleArray[i] + "'][id*='I1" + person + "'][id$='" + lastKeyword + "'][name!='frm1701" + "\\:" + "ModalDesc'][name!='frm1701" + "\\:" + "TaxFiler']:disabled").removeClass("disabledSchedules");
		}
		
		if (scheduleArray[i] == 'Pg8Sc7') {
			$("input[id*='txtPg8Sc7I1" + lastKeyword +"']").removeClass("disabledSchedules");
		}
		
		if (scheduleArray[i] == 'Pg8Sc8A1' || scheduleArray[i] == 'Pg8Sc8B1') {
			$("input[id*='" + scheduleArray[i] + "I5']").removeClass("disabledSchedules");
		}
		
		if(scheduleArray[i] == 'Pg6Sc6' || scheduleArray[i] == 'Pg7Sc6')
			$("input[id*='Pg6Sc6'][id*='I2" + person + "'][id$='" + lastKeyword + "'][name!='frm1701" + "\\:" + "ModalDesc'][name!='frm1701" + "\\:" + "TaxFiler']:disabled").removeClass("disabledSchedules");
			$("input[id*='Pg7Sc6'][id*='I36" + person + "'][id$='" + lastKeyword + "'][name!='frm1701" + "\\:" + "ModalDesc'][name!='frm1701" + "\\:" + "TaxFiler']:disabled").removeClass("disabledSchedules");
	}
}

