	//clearValues
	clearValue(type, typeTotal, person);

	
	//reset totalEXbyPerson && totalSPbyPerson on each clear
	//set to zero after executing clearValue()
	if (type === "EX"){ 
		totalEXbyPerson["TF"] = (person === "TF") ? 0 : totalEXbyPerson["TF"];
		totalEXbyPerson["S"] = (person === "S") ? 0 : totalEXbyPerson["S"];
