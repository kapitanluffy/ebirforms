        var item72 = removeCommaParenthesis("frm1701:txtPg3Pt5I72TotalIncomeTaxC1") + removeCommaParenthesis("frm1701:txtPg3Pt5I72TotalIncomeTaxC2");
        var item72Percentage = Math.round(item72 * 0.5);
        var errormessage = '';  
        
        errormessage = checkPercentage(50, 'frm1701:txtPg3Pt5I79LessPortion', 'frm1701:txtPg1Pt2I26IncomeTax', 'Item 79 must not be more than 50% of the sum of Items 72A and 72B.', 'return');
        
        var item76 = removeCommaParenthesis('frm1701:txtPg3Pt5I76TotalTaxC1') + removeCommaParenthesis('frm1701:txtPg3Pt5I76TotalTaxC2');
