    function computeQualifiedDependents(isAlert) {
        isAlert = isAlert == undefined ? false : true;
        var numberOfDependents = 0;
        var taxFilersAge = 0;
		isQualifiedDependent1 = false, isQualifiedDependent2 = false, isQualifiedDependent3 = false, isQualifiedDependent4 = false;

        var taxFilerBirthdaySplit = d.getElementById('frm1701:txtPg1Pt1I11Month').value.split('/') ;
        var spouseBirthdaySplit = d.getElementById('frm1701:ddlPg4Pt7I99Month').value.split('/');

        var taxFilersBirthdayMonth = taxFilerBirthdaySplit[0];
        var taxFilersBirthdayDay = taxFilerBirthdaySplit[1];
        var taxFilersBirthdayYear = taxFilerBirthdaySplit[2];
        var spouseBirthdayMonth = spouseBirthdaySplit[0];
        var spouseBirthdayDay = spouseBirthdaySplit[1];
        var spouseBirthdayYear = spouseBirthdaySplit[2];

       
        if (d.getElementById('frm1701:rdoPg1Pt1I24ClaimingYes').checked && d.getElementById('frm1701:rdoPg4Pt7I110Yes').checked == false
            && taxFilersBirthdayMonth != '' && taxFilersBirthdayDay != '' && taxFilersBirthdayYear != '') {
            taxFilersAge = calculateAge(convertToNumber(taxFilersBirthdayMonth) + "/" + convertToNumber(taxFilersBirthdayDay) + "/" + convertToNumber(taxFilersBirthdayYear));
        }
        else if (d.getElementById('frm1701:rdoPg4Pt7I110Yes').checked && d.getElementById('frm1701:rdoPg1Pt1I24ClaimingYes').checked == false
            && spouseBirthdayMonth != '' && spouseBirthdayDay != '' && spouseBirthdayYear != '') {
            taxFilersAge = calculateAge(convertToNumber(spouseBirthdayMonth) + "/" + convertToNumber(spouseBirthdayDay) + "/" + convertToNumber(spouseBirthdayYear));
        }
        
        for (var i = 1; i < 5; i++) {
            if (d.getElementById('frm1701:txtPg4Pt7ALastName' + i).value !== "" && d.getElementById('frm1701:txtPg4Pt7AFirstMiddleName' + i).value !== "") {
                var dob = d.getElementById('frm1701:txtPg4Pt7AMonth' + i).value;

                if (isAlert && dob == "") {
                    alert("Date of Birth of Dependent is required. Please enter a valid date on Part VIIA");
                    break;
                } else if (dob != "") {
                    var age = calculateAge(dob);
					//Added as of 03/28/2017 by Jhong
					//If mentally or physically incapacitated is being marked.  Any age will be accepted
					if (d.getElementById('frm1701:chkPg4Pt7AIncapacitated' + i).checked) {
                        numberOfDependents++;
                        if (i == 1) {
                            isQualifiedDependent1 = true;
                        } else if (i == 2) {
                            isQualifiedDependent2 = true;
                        } else if (i == 3) {
                            isQualifiedDependent3 = true;
                        } else if (i == 4) {
                            isQualifiedDependent4 = true;
                        }
                    }
                    else if ((age <= 21 || (age > 21 && d.getElementById('frm1701:chkPg4Pt7AIncapacitated' + i).checked)) && (age < taxFilersAge && taxFilersAge != 0)) {
                        numberOfDependents++;

                        if (i == 1) {
                            isQualifiedDependent1 = true;
                        } else if (i == 2) {
                            isQualifiedDependent2 = true;
                        } else if (i == 3) {
                            isQualifiedDependent3 = true;
                        } else if (i == 4) {
                            isQualifiedDependent4 = true;
                        }
                    } else if (isAlert && age > taxFilersAge && taxFilersAge != 0) {
