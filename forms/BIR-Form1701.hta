function deleteAttachment() {

	var type = d.getElementById("currentTypeView").value;
	var current = parseInt(d.getElementById("attachmentCurrent").value);

	var deletedItem = "";
	var lastItem = "";
	var total = -1;

	type = type.substring(0, 2);

	deletedItem = (type === "EX") ? (totalEXArray[current - 1]) : (totalSPArray[current - 1]);
	lastItem = (type === "EX") ? (totalEXArray[totalEXArray.length - 1]) : (totalSPArray[totalSPArray.length - 1]);

	total = (type === "EX") ? (totalEXArray.length) : totalSPArray.length;

	if (total > 1){
		
		var confirmMsg = "Are you sure you want to delete this attachment? This is irreversible. ";
		//03/16/17 edited message
		//confirmMsg = (lastItem !== deletedItem) ? confirmMsg + "Note: Attachment " + lastItem + " (last attachment) will become " + deletedItem +"." : confirmMsg;
		confirmMsg = (lastItem !== deletedItem) ? confirmMsg + "" : confirmMsg;
		
		// new rule on deleting attachment 03/09/14
		/*
			if (total===1){
				
				var typeCompete = (type==="EX") ? "Exempt Tax Regime" : "Special/Preferential Rate";
				confirmMsg += "Zero attachment for "+typeCompete+".";
				
				//uncheck radio button
				if (type === "EX"){
					d.getElementById('frm1701:rdoPg1Pt1I22IncomeExemptYes').checked = false;
				}else{
					d.getElementById('frm1701:rdoPg1Pt1I23IncomeSpecialYes').checked = false;
				}

				// to activate checkbox
				document.onmouseup();
			}
		*/
		
		if (confirm(confirmMsg) === true) {
			
			//call these updates before resetting currentTypeView
			
			//update disabledInputs on delete
			updateDisabledinputIDsOnDelete(d.getElementById("currentTypeView").value);
			//update txtCtrmodal**
			deleteAttachemntModalCounter(d.getElementById("currentTypeView").value);
			
			//update enabled links // for modals
			for (var i = 0; i < enabledlinkIDs.length; i++) {
                //Convert to $.inArray, indexOf displays error 05/08/2014
				var isSameTYPE = $.inArray(d.getElementById("currentTypeView").value.toString(), enabledlinkIDs); //enabledlinkIDs.indexOf(d.getElementById("currentTypeView").value.toString());
				
				if (isSameTYPE >-1){
					enabledlinkIDs_Add(false,enabledlinkIDs[i]);
				}
			
			}
			//update disabled textbox // for modals
			for(var i=0; i < disabledinputIDs.length ; i++){
			
				var inputSet = disabledinputIDs[i].toString().split(",");
				var isSameTYPE = inputSet[0].indexOf(d.getElementById("currentTypeView").value.toString());
				
				if (disabledinputIDs[i].toString().trim() !== ""){
				
					if (isSameTYPE >-1){
						
						disabledinputIDs.splice(i, 1);
					}
				}
			}
			
			if (type === "EX") {
				// Remove last item from totalEXArray
				totalEXArray.splice(-1, 1);
				totalEXbyPerson["TF"] = (totalEXbyPerson["TF"]==null || totalEXbyPerson["TF"]<=0 ) ? 0 : totalEXbyPerson["TF"]-1;
				totalEXbyPerson["S"] = (totalEXbyPerson["S"]==null || totalEXbyPerson["S"]<=0) ? 0 : totalEXbyPerson["S"]-1;
				
			}
			else if (type === "SP") {
				// Remove last item from totalSPArray
				totalSPArray.splice(-1, 1);
				totalSPbyPerson["TF"] = (totalSPbyPerson["TF"]==null || totalSPbyPerson["TF"]<=0) ? 0 : totalSPbyPerson["TF"]-1;
				totalSPbyPerson["S"] = (totalSPbyPerson["S"]==null || totalSPbyPerson["S"]<=0) ? 0 : totalSPbyPerson["S"]-1;
			}

			//delete attachement container
			$("#" + deletedItem).remove();
			$("#modal" + deletedItem).remove();
			
			var isExempt = (type==="EX")? true : false;
			
			if (total === 1) {
			
				//recompute but just set to zero	
				recomputeForMainPages(true, 'TF',isExempt);
				recomputeForMainPages(true, 'S',isExempt);
				
				window.setTimeout("processBack();",200);
				
			}else{
					
				changeIdTransferredAttachment(type, lastItem, deletedItem);
				
				//reset total
				total = (type === "EX") ? (totalEXArray.length) : totalSPArray.length;
				setView(type, total, total);
				displayAttachmentByPage(1);
				
				recomputeForMainPages(false, PERSON,isExempt);
			}
			
			//udpate frm1701:txtAttachmentTypes for reloading
			attachmentTypes_Update();
			
			//03/16/14
			addFocusBlurEvent("attachment" + type);
			
		}
	}else{
		
		var item = (PERSON === "TF") ? 22 : 107; 
		item = (type === "SP") ? (item * 1)+1 : item;
		alert("Please check item "+item.toString()+ " 'No' mark box to delete remaining attachment.");
	}

}
	
// This function changes the id of the div that matches lastItem with deletedItem
// Example: If lastItem is EX10 and deletedItem is EX2, 
//			the div with the id EX10 will be changed to EX2
function changeIdTransferredAttachment(attachType, lastItem, deletedItem) {

	var replaceRegEx = new RegExp();

	if (attachType === "EX") {
	
		replaceRegEx = new RegExp(lastItem, "g");

		$("#attachmentEX").html($("#attachmentEX").html().replace(replaceRegEx, deletedItem));
		$("#attachmentModalEX").html($("#attachmentModalEX").html().replace(replaceRegEx, deletedItem));
	
	}else if (attachType === "SP") {
	
		replaceRegEx = new RegExp(lastItem, "g");

		$("#attachmentSP").html($("#attachmentSP").html().replace(replaceRegEx, deletedItem));
		$("#attachmentModalSP").html($("#attachmentModalSP").html().replace(replaceRegEx, deletedItem));
	}
}


var attachmentEnabledSpouse  = [];
var attachmentEnabledSpouseAddCount = -1;

//if PERSON is taxfiler disable elements with name 'frm1701:Spouse' && vice versa
//rule 1 : IDs with 'C2' == spouse;; 'C1' == taxfiler
function disableSpouseForAttachments(person){

	PERSON = person;

	var isSpouse = (person.toUpperCase()==="S") ? true : false;
	
	var allSpouse = d.getElementById(TYPE).getElementsByTagName('input');
	
	//get all enabled spouse ONCE for each TYPE
	if (attachmentEnabledSpouseAddCount !== 1){
		for (var i=0; i < (allSpouse.length); i++) {
			
			if (   (allSpouse[i].name === "frm1701:Spouse")&&(allSpouse[i].disabled === false)   ){
				attachmentEnabledSpouse.push(allSpouse[i]);
			}
			if (i === (allSpouse.length-1) ){
				attachmentEnabledSpouseAddCount = 1;
			}
		}
	}

	//changeID
	//anyway, 'EX1' lang naman ung difference nila sa ID ^_^
	//C1 for taxFiler , C2 for Spouse
	for (var i=0; i < (attachmentEnabledSpouse.length); i++) {

		var typeIndex = (attachmentEnabledSpouse[i].id.indexOf('EX')>0) ? (attachmentEnabledSpouse[i].id.indexOf('EX')) : attachmentEnabledSpouse[i].id.indexOf('SP');
			
		var idSpouse= attachmentEnabledSpouse[i].id.substring(0, typeIndex);

		//disable enable all spouseControls
		d.getElementById(idSpouse + TYPE).disabled = !isSpouse;

		//disable enable all taxFilerControls
		var idTaxpayer = idSpouse.substring(0,idSpouse.length-1);	
		var num = idSpouse.substring(idSpouse.length-1,idSpouse.length) ;
		idTaxpayer = idTaxpayer+(num-1)+TYPE;

		if (d.getElementById(idTaxpayer)!= null){
			d.getElementById(idTaxpayer).disabled = isSpouse;
		}
		
	}
	
	validateModalState(false);
	disableAddMoreLinkPg5m(person);

    //Close inputs in a row if OTHERS 05/08/2014
	closeOthersRow();
}

//Close inputs in a row if OTHERS 05/08/2014
function closeOthersRow() {
    var othersFields = $('#ATTACHMENT input[type=text][value="OTHERS"]:disabled').not('[name*= "1m"]');
    
    for (i = 0; i < othersFields.length; i++) { 
        var inputID = othersFields[i].name .split("C1")[0];
        var type = othersFields[i].id .split("C1")[1];
        
        if(d.getElementById(inputID + "C2" + type)  !== null) { d.getElementById(inputID + "C2" + type).disabled = true; }
        if(d.getElementById(inputID + "C3" + type)  !== null) { d.getElementById(inputID + "C3" + type).disabled = true; }
        if(d.getElementById(inputID + "C4" + type)  !== null) { d.getElementById(inputID + "C4" + type).disabled = true; }
    }
}

//clear value for all EX or SP	
function clearValue(type, typeTotal, person){
	
	var lastContainer = type.toString() + typeTotal.toString();
	var allSpouse = d.getElementById(lastContainer).getElementsByTagName('input');
	
	var inputs = [];
	
	//get all inputs with name frm1701:Spouse
	for (var i=0; i < allSpouse.length; i++) {
				
		if (allSpouse[i].name === "frm1701:Spouse"){
			
			var mainID = allSpouse[i].id .substring(0, (allSpouse[i].id.indexOf(type)));
			
			//duplicate inputs to get all EX
			//ex. EX1 to EX4
			for (var x=1; x <= typeTotal; x++) {
				inputs.push(d.getElementById(mainID + type + x));
			}
		}
	}
	
	//clear values of all inputs, this depends on person
	for (var i=0; i < inputs.length; i++) {
		
		//clear SPOUSE inputs
		if (person === "S"){

			if (inputs[i].maxLength === 12){
				inputs[i].value = "0";
			}else if (inputs[i].maxLength === 5){
				inputs[i].value = "0.0";
			}else{
				inputs[i].value = "";
			}

		}
		//clear TAXFILER inputs
		//manipulate Spouse ID to get Taxfiler ID
		else{
			
			var typeIndex = (inputs[i].id.indexOf('EX')>0) ? (inputs[i].id.indexOf('EX')) : inputs[i].id.indexOf('SP');
			var type = inputs[i].id.substring(typeIndex, inputs[i].id.length);

			var idSpouse= inputs[i].id.substring(0, typeIndex);
			var  idTaxpayer = idSpouse.substring(0,idSpouse.length-1);	
			var  num = idSpouse.substring(idSpouse.length-1,idSpouse.length) ;
				idTaxpayer = idTaxpayer+(num-1)+type;
				
			if (d.getElementById(idTaxpayer)!= null){
			
				if (d.getElementById(idTaxpayer).maxLength === 12){
					d.getElementById(idTaxpayer).value= "0"; 
				}else if (d.getElementById(idTaxpayer).maxLength === 5){
					d.getElementById(idTaxpayer).value= "0.0"; 
				}else{
					d.getElementById(idTaxpayer).value= ""; 
				}
				
			}
			
		}
	}

	var isExempt = (type.indexOf("EX") > -1)? true : false;
	recomputeForMainPages(true, person, isExempt);
}

