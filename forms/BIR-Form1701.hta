	//===========================================================================================================================
	// for iterating attachment, use this instead of moveValue();
	function computeCollectedValue(fromID, toID){
		
		var id = fromID.toString().split(":");
		
		var fromCollection = $("input[id*=" + ( id[0]+"\\:"+id[1] ) + "]");
		
		var total = 0;

		for (var i = 0; i < fromCollection.length; i++) {
			
			//do not compute textbox with id TYPE, they are the source HTML
			if (fromCollection[i].id.indexOf("TYPE") === -1){
				
				total = convertToFloat(removeCommaParenthesis(fromCollection[i].id )) + total;  
				//alert(i+ ": " + convertToFloat(fromCollection[i].value)  + "  || id "+fromCollection[i].id);
			}
		}
		d.getElementById(toID).value = NegativeValue(formatCurrencyWOC(Math.round(total)));
	}
	
	
