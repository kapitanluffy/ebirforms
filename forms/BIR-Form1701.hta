        inputCount = input.length - subtotalTextBoxNumber;
        var row = 5;
        var rowToBeDisabled = parseInt((inputCount / columnNumber) - 1);
        var j = rowIndexStart;

        if (j < inputCount) {
            for (var i = rowIndexStart; i < (rowIndexStart + (columnNumber * rowToBeDisabled)); i++) {
                if (input[j].value == '' ||  input[j + 1].value == "0"
                    || (input[j + columnNumber].value == "OTHERS" && input[j + columnNumber].disabled == true)) {
                    if (input[j].value == '' && input[j + 1].value !== "0" && input[j + 1].value !== "" && input[j + 1].value !== "undefined") {
                        alert("Please enter a valid Year Incurred on Page 8 Schedule " + tableID.substring(5, 7) + ".1 Item " + (row));
                    }
                    else if (input[j].value != '' && input[j + 1].value == "0") {
                        alert("Please enter a valid Amount on Page 8 Schedule " + tableID.substring(5, 7) + ".1 Item " + (row));
                    }

                    break;
                }
                else {
                   
                    input[i + columnNumber].disabled = false;
                    input[i + columnNumber].style.backgroundColor = '#ffffff';
                    
                    if(input2[i + columnNumber].id.indexOf('NetOperatingLoss') == -1) {
                        input2[i + columnNumber].disabled = false;
                        input2[i + columnNumber].style.backgroundColor = '#ffffff';
                    }
                }

                if (i == j + columnNumber - 1) {
                    j = j + columnNumber;
                    row++;
                }
            }
        }
    }
	
