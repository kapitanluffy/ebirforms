<script type="text/javascript" name="dynamicModalScript">

function changeSpouseTaxFilerState(container){

	//check if spouse should be enabled or disabled
	var isSpouseDisabled = (d.getElementById('frm1701:txtIsSpouseDisabled').value === "true") ? true : false;

	//for Attachments
	var isOnAttachment =  (d.getElementById("frm1701:txtCurrentPage").disabled) ? true : false;

	//enable // disable spouse textboxes here
	var t = container.getElementsByTagName('input');

	for (var x = 0; x < (t.length ); x++) {
	
		if (t[x].type === "text" && t[x].maxLength === 12){
			
			//======================================================
			//attachment
			//======================================================
			if (isOnAttachment){
				
				isSpouseDisabled =  (PERSON === "S") ? true : false;
				
				//spouse textboxes
				if (t[x].name === 'frm1701:Spouse'){
					//spouse textboxes
					t[x].disabled = !isSpouseDisabled;
					
				//taxfiler textboxes
				}else{
					//taxFiler textboxes
					t[x].disabled = isSpouseDisabled;
				}				
			}
			//======================================================
			//mainPages
			//======================================================
			else{
				//04/07/14
				//implement next build
				var isTaxfilerDisabled = (d.getElementById('frm1701:txtIsTaxFilerDisabled').value === "true") ? true : false;
				
				if (d.getElementById('frm1701:rdoPg1Pt1I20DeductionOSD').checked || d.getElementById('frm1701:rdoPg4Pt7I105OSD').checked) {
					
					var idArr = ["Pg6Sc6", "Pg7Sc6", "Pg8Sc7"];

					var ID = t[x].id.split(",")[0];
					ID = ID.split(":")[1].substring(3, 9);

					if ($.inArray(ID, idArr) > -1) {
					
						if(d.getElementById('frm1701:rdoPg1Pt1I20DeductionOSD').checked) {
							isTaxfilerDisabled = true;
							isSpouseDisabled = false;
						}

						if (d.getElementById('frm1701:rdoPg4Pt7I105OSD').checked) {
							isTaxfilerDisabled = false;
							isSpouseDisabled = true;
						}
					}
				}
				
				//spouse textboxes
				if (t[x].name === 'frm1701:Spouse'){
					//disable textbox
					t[x].disabled = isSpouseDisabled;
					
					//set value to zero
					if (isSpouseDisabled){
						t[x].value = "0";
					}
					
				//taxfiler textboxes
				}else{

					t[x].disabled = isTaxfilerDisabled;
					
					//set value to zero
					if (isTaxfilerDisabled){
						t[x].value = "0";
					}
				}
			}
			
			//======================================================
			//add comma
			//======================================================
			t[x].value = NegativeValue(formatCurrencyWOC(t[x].value));
		}
	}
	
}

//this will enable link when textbox is
//.. if accepts numeric, must be greater than zero
//.. if accepts alphaN, must not be empty string
//can accept 'this' or multitple IDs
function disableLinkIfEmpty(e, linkID){

    var link = d.getElementById(linkID);

	var IDs = [];
	
	var isValid = true;
	var changeLinkState = true;
	
	var paramType = (typeof e);
	
	var isTaxfilerDisabled = (d.getElementById('frm1701:txtIsTaxFilerDisabled').value.toString() === "true") ? true : false;
	var isSpouseDisabled = (d.getElementById('frm1701:txtIsSpouseDisabled').value.toString() === "true") ? true : false;


	
	
	//if parameter is string, set of IDs
	if (paramType.toString() === "string"){
		
		IDs = e.split(",");
		
		if (IDs.length > 0){
			
			//if MOD is OSD
			if (d.getElementById('frm1701:rdoPg1Pt1I20DeductionOSD').checked || d.getElementById('frm1701:rdoPg4Pt7I105OSD').checked) {
				
				var idArr = ["Pg6Sc6", "Pg7Sc6", "Pg8Sc7"];

				var ID = e.split(",")[0];
				ID = ID.split(":")[1].substring(3, 9);

				if ($.inArray(ID, idArr) > -1) {
					if(d.getElementById('frm1701:rdoPg1Pt1I20DeductionOSD').checked) {
						isTaxfilerDisabled = true;
						isSpouseDisabled = false;
					}

					if (d.getElementById('frm1701:rdoPg4Pt7I105OSD').checked) {
						isTaxfilerDisabled = false;
						isSpouseDisabled = true;
					}
				}
			}
			
			//store IDs here,
			//add spouse ID which is not included in parameter
			var completeIDs = [];
			
			for (var x=0; x < IDs.length; x++) {
				
				completeIDs.push(IDs[x]);
			
				//add SpouseID
				var spouseID = getSpouseIDviaTaxfilerID(IDs[x]);
				if (spouseID != null ){
					if (spouseID.toString().trim() != ""){
						completeIDs.push(spouseID);
					}
				}
				
				if ( (d.getElementById(IDs[x]) != null)  &&   (d.getElementById(IDs[x]).value.toString().trim() === "OTHERS")  &&    (d.getElementById(IDs[x]).disabled)  ){
					
					changeLinkState = false;
				}	
			}
			
			if (changeLinkState){

				for (var x=0; x < completeIDs.length; x++) {

					var ctrl = d.getElementById(completeIDs[x]);
					
					if (ctrl != null &&  ctrl.type=== "text"){
					
						//validate description
						if ( ctrl.maxLength !== 12  ){
							
							if (ctrl.value.toString().trim() === ""){
								isValid = false;
							}
							
						//validate amounts
						}else{
							
							//for page 5
							//this only validates Compensation Income
							if ( ctrl.id.indexOf("Pg5Sc1") > -1){
								
								if ( ctrl.id.indexOf("ComIncome") > -1 && ctrl.value <=  0 ){
									isValid = false;
								}
								
							//for other row validation
							}else{
								
								isValid = isAmountValid(ctrl);
								
								if (!isValid){
									break;
								}
								
							}
						}

					}
				}
			}
		}
	//if parameter is 'this' [type of object]
	}else{
		
		if (   (e.maxLength !== 12 && e.value.toString().trim() === "") || (e.maxLength === 12 && e.value <= 0)  ){
			isValid = false;
		}
	}

	link.disabled = !isValid;

	//save enabled link to frm1701:txtEnabledLinks
	//will be used for reloading of data from XML
	if (isValid){
		enabledlinkIDs_Add(true, linkID);
	}else{
		enabledlinkIDs_Add(false, linkID);
	}
	
	//for Attachments
	if (isOnAttachment){
	
		isTaxfilerDisabled = (PERSON === "S") ? true  : false;
		isSpouseDisabled = (PERSON === "TF") ? true  : false;
	}
	
	validateEnabledIDs(IDs, isTaxfilerDisabled, isSpouseDisabled);
}

