//this will validate ids per row
//call this on disableLinkIfEmpty()
function validateEnabledIDs(IDs, isTaxfilerDisabled, isSpouseDisabled){
	
	//get all IDs main ID by rows
	//ex. 'Pg10Sc11AI59'
	var rowID = [];
	for (var x=0; x < IDs.length; x++) {

		var IDsDescription = getDesription(IDs[x].toString(),  getIDDescriptionOnly(IDs[x]));
		//get : 'Pg10Sc11AI59'
		var IDsMainID = IDsDescription[0];
		//get : 'EX1'
		var IDsAttachementType = IDsDescription[1];
		
		var isIDAdded = ($.inArray(IDsMainID, rowID) == -1) ? false : true
		
		if (!isIDAdded){

			rowID.push(IDsMainID);
		}
	}
	
	//get ID per ROW
	var setOfIDPerRow = [];
	for (var i=0; i < rowID.length; i++) {
		
		//get all IDs per row
		var str = "";
		
		for (var x=0; x < IDs.length; x++) {
		
			if (IDs[x].indexOf(rowID[i]) > -1 ){
				str += IDs[x] + ",";

				//add SpouseID
				var spouseID = getSpouseIDviaTaxfilerID(IDs[x]);
				if (spouseID != null){
					str+=spouseID + ",";
				}
				
			}
		}

		setOfIDPerRow[setOfIDPerRow.length] = str;
	}
	
	//set of id per ROW
	for (var i=0; i < setOfIDPerRow.length; i++) {
	
		var id = setOfIDPerRow[i].split(',');
		var amount = [];
		var description = [];
		
		//collect all textbox which is type(amount) && type(description)
		for (var x=0; x < id.length; x++) {
			
			if (id[x].toString().trim() !== "" && d.getElementById(id[x]) != null && d.getElementById(id[x]).type ==="text" ){
				
				var IDs = d.getElementById(id[x]);
				
				if (IDs.maxLength == 12 ){
				amount.push(IDs);
				}else{
				description.push(IDs);
				}
			}
		}
		
		var isDescriptionEmpty = false; 
		var isAmountZero = true; 
		
		//validate amount
		for (var x=0; x < amount.length; x++) {
		
			isAmountZero = !isAmountValid(amount[x]);
			
			if (!isAmountZero){
				break;
			}
		}
		
		//validate description
		
		//for rows with legal basis
		if (description.length == 2){
			

			if ( (description[0].value.toString().trim() !== "") && description[1].value.toString().trim() !== "" ){
				
				isDescriptionEmpty = false; 
			}else{
				isDescriptionEmpty = true;
			}
			
			if ( (description[0].value.toString().trim() === "") && description[1].value.toString().trim() === "" ){
					
				isDescriptionEmpty = true;
			}
		//other common rows, mostly with 1 description 
		}else{
		
			for (var x=0; x < description.length; x++) {
				
				if (description[x].value.toString().trim() === ""){
					
					isDescriptionEmpty = true;
				}else{
					
					isDescriptionEmpty = false; 
				}
			}
		}
		var action = "ADD";
		var IDsParam = [];
		var isAmount = false;
		
		//if isAmountZero(0) && isDescriptionEmpty(emptyString)
		//if isAmountZero>=0 && isDescriptionEmpty 
		
		if ( (isAmountZero &&  isDescriptionEmpty) || (isAmountZero==false &&  isDescriptionEmpty==false) ){
			
			action = "REMOVE";
			updateScheduleErrorID(amount,true,action);
			updateScheduleErrorID(description,false,action);
			
			//for page 5
			if (  (id[id.length -1].indexOf("Pg5Sc1")>-1)  &&  !(d.getElementById('Sc1I4rdoSpanOther').style.display == "none")  ){
				
				//if OTHERS is not hidden
				//do not allow to enable next row
				isAmountZero = true;
			}
			
			//enable next row if current row is already valid
			if (isAmountZero==false &&  isDescriptionEmpty==false){
			
				if (i != setOfIDPerRow.length-1){

					enableNextRow(setOfIDPerRow[i+1], isTaxfilerDisabled, isSpouseDisabled);
				}
			}
			
		}else{
			
			//if amount is zero & description is not empty
			//amount is mandatory
			if ( isAmountZero &&  isDescriptionEmpty==false ){
				action = "ADD";
				IDsParam = amount;
				isAmount = true;
			//if description is empty & amount is greater than zero
			//description is mandatory
			}else if ( isDescriptionEmpty &&  isAmountZero==false ){
				action = "ADD";
				IDsParam = description;
				isAmount = false;
			}

			updateScheduleErrorID(IDsParam,isAmount,action);
		}
		
	}

}

