 //call this when deleting attachment to update ID Containers
function updateDisabledinputIDsOnDelete(type){

	var deletedInput = -1;
	var deletedLink = -1;
	
	for (var i=0; i < disabledinputIDs.length; i++) {
		
		if (disabledinputIDs[i].toString().split(",")[0].indexOf(type) > -1){
		
			deletedInput = i;
		}
	}
	
	disabledinputIDs.splice(deletedInput, 1);
	//update frm1701:txtDisabledInputs after deletion
	storeDisabledinputIDs();
	
	for (var i=0; i < enabledlinkIDs.length; i++) {
	
		if (enabledlinkIDs[i].toString().indexOf(type) > -1 ){

			deletedLink = i;
		}
	}
	
	enabledlinkIDs.splice(deletedLink, 1);
	//update frm1701:txtDisabledInputs after deletion
	storeEnabledlinkIDs();
}
	
	
function disableAddMoreLinkPg5m(person){

	var i3LinkID = 'frm1701:lnkPg5mScK1I3More'+TYPE;
	var i6LinkID = 'frm1701:lnkPg5mScK1I6More'+TYPE;
	var i8LinkID = 'frm1701:lnkPg5mScK1I8More'+TYPE;
	
	var pg5LinkID1 = 'frm1701:lnkPg5mScK';
	var pg5LinkID2  ='More'+TYPE;
	
	var addID = (person === "S") ? "1I" : "2I" ;

	//disabale all spouse links
	for(var i=3; i< 9 ; i++){
		
		//control itertion
		//do only numbers : 3, 6, & 8
		var counter = (i==3) ? 2 : 1;
		
		var id = pg5LinkID1+addID+i+pg5LinkID2;
		d.getElementById(id).disabled = true;
		
		i += counter ;
	}
}
	
