				}
			}

			if (radioID == "frm1701:rdoPg5Sc1I" + [a] + "Spouse" && "frm1701:txtPg5Sc1I" + [a] + "TaxWithheldC3" != "frm1701:txtPg5Sc1I4TaxWithheldC3") {
				if (d.getElementById("frm1701:rdoPg5Sc1I" + [a] + "Spouse").checked) {
					totalSpouse += parseInt(removeCommaParenthesis("frm1701:txtPg5Sc1I" + [a] + "ComIncomeC2"));
					totalTaxWithheldSpouse += parseInt(removeCommaParenthesis("frm1701:txtPg5Sc1I" + [a] + "TaxWithheldC3"));
				}
			}
				}
				else {
					if (radioID == "frm1701:rdoPg5Sc1I" + [a] + "Taxpayer") {
						if (d.getElementById("frm1701:rdoPg5Sc1I" + [a] + "Taxpayer").checked) {
							totalTaxPayer += parseInt(removeCommaParenthesis("frm1701:txtPg5Sc1I" + [a] + "ComIncomeC2"));
