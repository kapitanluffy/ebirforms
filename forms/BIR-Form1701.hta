		inputs[5].id = "frm1701:txtPg5Sc1I" + idX + "EmpTIN2C1";
		inputs[6].id = "frm1701:txtPg5Sc1I" + idX + "EmpTIN3C1";
		inputs[7].id = "frm1701:txtPg5Sc1I" + idX + "EmpTIN4C1";
		inputs[8].id = "frm1701:txtPg5Sc1I" + idX + "ComIncomeC2";
		inputs[9].id = "frm1701:txtPg5Sc1I" + idX + "TaxWithheldC3";
		*/
		x += 14;
		idX++;
	}
}

