function resetValuePart7A() {

	var control = d.getElementById('divPart7A').getElementsByTagName('input');

	if (d.getElementById('frm1701:rdoPg1Pt1I24ClaimingNo').checked && d.getElementById('frm1701:rdoPg4Pt7I110No').checked 
		|| ((d.getElementById('frm1701:rdoPg1Pt1I14StatusSingle').checked || d.getElementById('frm1701:rdoPg1Pt1I14StatusSeparated').checked
		|| d.getElementById('frm1701:rdoPg1Pt1I14StatusWidow').checked) && d.getElementById('frm1701:rdoPg1Pt1I24ClaimingNo').checked)
		|| (d.getElementById('frm1701:rdoPg1Pt1I7TypeEstate').checked || d.getElementById('frm1701:rdoPg1Pt1I7TypeTrust').checked)) {
		
		for (var i = 0; i < control.length; i++) {
			if (control[i].type == 'text' || control[i].type == 'checkbox') {
				control[i].value = '';

				control[i].disabled = true;
					
				if (control[i].type == 'checkbox') {
					control[i].checked = false;
				}
			}
		}
		d.getElementById('frm1701:txtPg1Pt1I24Dependent').value = 0;
		d.getElementById('frm1701:txtPg4Pt7I111Dependent').value = 0;
		
	}else {
	
		for (var i = 0; i < control.length; i++) {
			if (control[i].type == 'text' || control[i].type == 'checkbox') {
				control[i].disabled = false;

				if (control[i].type == 'checkbox') {
					$('#divPart7A').delegate('#frm1701' + "\\:" + control[i].id.substring(8, control[i].id.length), 'mousedown', Custom.pushed);
					$('#divPart7A').delegate('#frm1701' + "\\:" + control[i].id.substring(8, control[i].id.length), 'mouseup', Custom.check);
				}
			}
		}
	}
}


