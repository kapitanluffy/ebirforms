	function validateYearEnd(isReturn) {
		var element = d.getElementById("frm1701:txtPg1I1Year");
        var inputYear = 2000 + (element.value * 1);
		var inputMonth = (d.getElementById('frm1701:ddlPg1I1Month').value * 1) - 1;
        var currentDate = new Date();
        var errorMessage = "";
		 //**PAGE 1 VALIDATION**//

		//Year must not be null 
		if (element.value.trim() === "") {
		    errorMessage = "Please enter a valid Year on Page 1 Part 1 Item 1.";
		    element.focus();
        }
		else if (d.getElementById('frm1701:ddlPg1I1Month').value != "" || element.value != "") {
		    //Month and Year must be September 2013 onwards 
		     if (d.getElementById('frm1701:txtPg1I1Year').value * 1 < 13) {
		        errorMessage = "Valid input for the Month and Year should not be earlier than September 2013 on Page 1 Part 1 Item 1.";
		        element.value = '';
		        element.focus();
            }
            //else if (d.getElementById('frm1701:txtPg1I1Year').value * 1 > 17) {
            //    alert("Please use newer version if you wish to file for year 2018 and up.");
            //    element.value = '';
		    //    element.focus();
            //}
		    else if (d.getElementById('frm1701:txtPg1I1Year').value * 1 == 13 && d.getElementById('frm1701:ddlPg1I1Month').value < 9) {
		        errorMessage = "Valid input for the Month and Year should not be earlier than September 2013 on Page 1 Part 1 Item 1.";
		        d.getElementById('frm1701:ddlPg1I1Month').focus();
		    }
		        //Filing year  must be valid
		    else if ((d.getElementById("frm1701:rdoPg1I3ShortPeriodNo").checked === true) && (inputYear >= currentDate.getFullYear())) {
		            errorMessage = "Invalid Filing Year on Page 1 Part 1 Item 1.Year cannot be greater than or equal to current year.";
		            element.value = '';
		            element.focus();
		    }
		    else if ((d.getElementById("frm1701:rdoPg1I3ShortPeriodYes").checked === true)) {
		            if ((inputYear > currentDate.getFullYear())) {
		                errorMessage = "Year (Page 1 Item 2) cannot be greater than the current year when filing for Calendar Year.";
		                element.value = '';
		                element.select();
		            }
		            else if ((inputYear === currentDate.getFullYear()) && (inputMonth > currentDate.getMonth())) {
		                errorMessage = "Month (Page 1 Item 2) cannot be greater than  current month date when filing for Calendar Year  and  Short Period Return.";
		                d.getElementById('frm1701:ddlPg1I1Month').value = '';
		                d.getElementById('frm1701:ddlPg1I1Month').focus();
		            }
		            else if ((inputYear === currentDate.getFullYear()) && (inputMonth == 11)) {
		                errorMessage = "Month (Page 1 Item 2) cannot be equal to december  when filing for Calendar Year and  Short Period Return.";
		                d.getElementById('frm1701:ddlPg1I1Month').value = '';
		                d.getElementById('frm1701:ddlPg1I1Month').focus();
		            }
		            else if (inputMonth < 0) {
		                errorMessage = "(Page 1 Item 2) Month is invalid.";
		                d.getElementById('frm1701:ddlPg1I1Month').focus();
		            }
		    }
		    
		}
		
        if (element.value !== '' && (element.value * 1) > -1 && (element.value * 1) < 10) {
            element.value = '0' + element.value * 1;
        }
		
        if (isReturn) {
            return errorMessage;
        } else if (errorMessage !== "") {
            alert(errorMessage);
        }
    }

