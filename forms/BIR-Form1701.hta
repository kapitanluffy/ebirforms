//store enabled inputs here
var enabledinputIDs = [];

//save all enabled inputs during row validations
//during reloading, sched textboxes are disabled by default,
//enable them by using the IDs saved in 'frm1701:txtEnabledInputsOnValidation'
function update_enabledInputsOnValidation(id,action){

	if (action === "ADD"){
		var doAdd = true ;
		 
		for(var i = 0 ; i < enabledinputIDs.length ; i ++){
			
			if (enabledinputIDs[i] == id){
				doAdd = false;
				break;
			}
		}
		
		if (doAdd){
			enabledinputIDs.push(id);
		}
	}else{
	
		var deleteIndex = -1;
		
		for(var i = 0 ; i < enabledinputIDs.length ; i ++){
		
			if (enabledinputIDs[i] == id){
				deleteIndex = i;
				break;
			}
		}
		
		enabledinputIDs.splice(deleteIndex, 1);
	}
	
	//store it to textbox 
	//will be used for relaoding
	var str = "";
	for(var i = 0 ; i < enabledinputIDs.length ; i ++){
		
		//do not include radio button in page 5 (on frm1701:txtEnabledInputsOnValidation)
		//conflict during reloading
		if (enabledinputIDs[i].indexOf('rdoPg5Sc1') == -1){
			str += enabledinputIDs[i] + ",";
		}
	}
	
	d.getElementById('frm1701:txtEnabledInputsOnValidation').value = str;
}

function validateEnabledIDsOnRowValidation(){
	
	var isTaxfilerDisabled = (d.getElementById('frm1701:txtIsTaxFilerDisabled').value.toString() === "true") ? true : false;
	var isSpouseDisabled = (d.getElementById('frm1701:txtIsSpouseDisabled').value.toString() === "true") ? true : false;
	
	var enabledInputsContainer = d.getElementById('frm1701:txtEnabledInputsOnValidation');
	var setOfEnabledIDS = [];
	setOfEnabledIDS = enabledInputsContainer.value.toString().split(",");
	
	var state = false;
	var addSpouse = true;
	var addTaxfiler = true;
	
	if (setOfEnabledIDS.length !== 0){
		
		for (var i=0; i < setOfEnabledIDS.length; i++) {
		
			var ctrl = d.getElementById(setOfEnabledIDS[i]);
			if (ctrl != null){
				
				//if spouse need to open
				if (!isSpouseDisabled){
				
					//check if there is spouse
					if (ctrl.name == "frm1701:Spouse"){
						addSpouse = false;
					}
				}
				//if taxfiler need to open
				if (!isSpouseDisabled){
				
					//check if there is taxfiler
					if (ctrl.id.indexOf("Tax")>-1){
						addTaxfiler = false;
					}
				}
			}
		}
	}
	
	if (addSpouse){
	
		for (var i=0; i < setOfEnabledIDS.length; i++) {
			
			var spouseID = getSpouseIDviaTaxfilerID(setOfEnabledIDS[i]);
			if (d.getElementById(spouseID)!=null){
				setOfEnabledIDS.push(spouseID);
			}
		}
	}
	
	if (addTaxfiler){
	
		for (var i=0; i < setOfEnabledIDS.length; i++) {
			
			var taxfilerID = getTaxfilerIDviaSpouseID(setOfEnabledIDS[i]);
			if (d.getElementById(taxfilerID)!=null){
				setOfEnabledIDS.push(taxfilerID);
			}
		}
	}
	
	for (var i=0; i < setOfEnabledIDS.length; i++) {

		var ctrl = d.getElementById(setOfEnabledIDS[i]);
		
		if (ctrl != null){
			
			enabledinputIDs.push(ctrl.id);	

			//spouse textboxes
			if (ctrl.name == "frm1701:Spouse"){
			
				ctrl.disabled = isSpouseDisabled;
			}
			//taxfiler textboxes
			if (ctrl.id.indexOf("Tax")>-1){
				ctrl.disabled = isTaxfilerDisabled;
			}

		}		
	}
}

function getTaxfilerIDviaSpouseID(taxfilerID){
	
	var isOnAttachment  = (d.getElementById('frm1701:txtCurrentPage').disabled) ? true: false;
	var spouse = "";
	var spouseDescID = "TaxPayerC1";
	var  spouseID = "";
	
	//for mainPages
	if (!isOnAttachment){
	
		if (taxfilerID != null){
		
			spouse = taxfilerID.substring( 0,taxfilerID.indexOf("Spouse"));
			
			spouseDescID = (d.getElementById(spouse + spouseDescID) != null) ? spouseDescID : "TaxSumPayerC1" ;
			
			//for page 8  schedule 7
			spouseDescID = (d.getElementById(spouse + spouseDescID) != null) ? spouseDescID : "TaxPayer" ;
			
			spouseID = (d.getElementById( spouse + spouseDescID ) != null) ? ( spouse + spouseDescID ) : null;		
		}
		
	}else{

		//try C4 to C3
		//case for page 4m (row with Description and Legal Basis)
		if (taxfilerID.indexOf("C4")>-1){
	
			spouseDescID = taxfilerID.replace(/C4/g, "C3");
			spouseID = (d.getElementById( spouseDescID ) != null ) ? ( spouseDescID ) : null;
	
		}else if (taxfilerID.indexOf("C3")>-1){
			
			//spouseDescID is already the full id
			spouseDescID = taxfilerID.replace(/C3/g, "C2");
			spouseID = (d.getElementById( spouseDescID ) != null) ? ( spouseDescID ) : null;
		}
	}
	
	return spouseID;
}

function getSpouseIDviaTaxfilerID(taxfilerID){
	
	var isOnAttachment  = (d.getElementById('frm1701:txtCurrentPage').disabled) ? true: false;
	var spouse = "";
	var spouseDescID = "SpouseC2";
	var  spouseID = "";
	
	//for mainPages
	if (!isOnAttachment){
		
		spouse = taxfilerID.substring( 0,taxfilerID.indexOf("Tax"));
		
		spouseDescID = (d.getElementById(spouse + spouseDescID) != null) ? spouseDescID : "SpouseSumC2" ;
		
		//for page 8  schedule 7
		spouseDescID = (d.getElementById(spouse + spouseDescID) != null) ? spouseDescID : "Spouse" ;
		
		spouseID = (d.getElementById(  spouse + spouseDescID ) != null && d.getElementById(  spouse + spouseDescID ).name == "frm1701:Spouse" ) ? ( spouse + spouseDescID ) : null;
		
	}else{
		
		//try C3 to C4
		//case for page 4m (row with Description and Legal Basis)
		if (taxfilerID.indexOf("C3")>-1){
	
			spouseDescID = taxfilerID.replace(/C3/g, "C4");
			spouseID = (d.getElementById( spouseDescID ) != null && d.getElementById( spouseDescID ).name == "frm1701:Spouse") ? ( spouseDescID ) : null;
	
		}else if (taxfilerID.indexOf("C2")>-1){
			
			//spouseDescID is already the full id
			spouseDescID = taxfilerID.replace(/C2/g, "C3");
			spouseID = (d.getElementById( spouseDescID ) != null && d.getElementById( spouseDescID ).name == "frm1701:Spouse") ? ( spouseDescID ) : null;
		}
	}

	return spouseID;
}

function getScheduleError(id, isAmount){
	
	var desc = getIDDescriptionOnly(id);
	
	var textboxDescription = getDesription(id, desc);
	//get : 'Pg10Sc11AI59'
	var mainID = textboxDescription[0];
	//get : 'EX1'
	var textboxAttachementType = textboxDescription[1];
	
	var page = mainID.substring(2,mainID.indexOf("Sc"));
	
	//amount, description, legalbasis
	var type = getIDDescriptionOnly(id);
	
	//all possible TYPE
	if (isAmount){
	
		type = "Amount";
		//type = (  (type.indexOf("Tax") > -1)||(type.indexOf("Spouse") > -1) && isAmount ) ? "Amount" : type;
	}else{
		
		type = (type.indexOf("cription") > -1) ? "Description" : type;
		type = (type.indexOf("Desc") > -1) ? "Description" : type;
		type = (type.indexOf("mount") > -1) ? "Amount" : type;
		type = (type.indexOf("Nature") > -1) ? "Nature of Income" : type;
		type = (type.indexOf("mortization") > -1) ? "Amortizations" : type;
		type = (type.indexOf("Incurred") > -1) ? "Year Incurred" : type;
		type = (type.indexOf("egal") > -1) ? "Legal Basis" : type;
		//case for page 10
		type = (  (type.indexOf("Tax") > -1)||(type.indexOf("Spouse") > -1)  ) ? "Description" : type;
		type = (type.indexOf("Special") > -1) ? "Description" : type;
		//case for page 5
		type = (type.indexOf("NameOfEmp") > -1) ? "Name of Employer" : type;
		type = (type.indexOf("EmpTIN") > -1) ? "Employer's TIN Number" : type;
	}	
	
	var phrase = (type==="Amount") ? " should not be zero." : " should not be empty.";
	
	//for page 4m schedule I ,, since there is 2 'I' in ID
	var addToIndex = (id.indexOf("Pg4mScI")>-1) ? 3 : 1;
	//get Item #
	var item = mainID.substring(mainID.indexOf("Sc")+addToIndex, mainID.length);
	item = item.substring(item.indexOf("I")+1, item.length);
	var sched = mainID.substring(mainID.indexOf("Sc")+2,mainID.indexOf("I"+item));
	
	var errorMessage = "Page " +page+ " Schedule " +sched+ " "+"Item " +item+" "+ type + phrase;

	return errorMessage;
}

var scheduleErrorList  = [];

function updateScheduleErrorID(idCollection,isAmount, action){
	
	//get all error message for all IDs
	var errorMessageCollection = [];

	for (var i=0; i < idCollection.length; i++) {
	
		var errorMessage = getScheduleError(idCollection[i].id,isAmount);
		
		errorMessageCollection.push(errorMessage+","+idCollection[i].id);
	
	}
	
	//adding errorMessage in scheduleErrorList[] (global variable)
	if (action.toString().toUpperCase() === "ADD"){
	
		//add all error message in scheduleErrorList
		for (var x = 0; x <  errorMessageCollection.length; x++){
			
			if (scheduleErrorList.length === 0){
			
				scheduleErrorList.push(errorMessageCollection[x]);
			
			}else{
				
				var doAdd = true;
				
				for (var i = 0; i <  scheduleErrorList.length; i++){
					//ADD IF scheduleErrorList does NOT contain current errorMessage
					if (scheduleErrorList[i] == errorMessageCollection[x]){
						
						doAdd = false;
						i = scheduleErrorList.length - 1;
					}
				}
				
				if (doAdd){
					
					scheduleErrorList.push(errorMessageCollection[x]);
				}
			}
		}
	
	//removing errorMessage in scheduleErrorList[] (global variable)
	}else{
		//find errorMessage in scheduleErrorList[]
		for (var x = 0; x <  errorMessageCollection.length; x++){
		//alert("remove.. "  + errorMessageCollection[x])
			for (var i = 0; i <  scheduleErrorList.length; i++){
			
				//REMOVE IF scheduleErrorList does NOT contain current errorMessage
				if (scheduleErrorList[i] == errorMessageCollection[x]){
					scheduleErrorList[i] = "";
				}
				if (scheduleErrorList[i].toString().trim() === ""){
					scheduleErrorList.splice(i,1);
				}
			}
			
		}
	}
	
	
}

function getIDDescriptionOnly(controlID){

	var id = controlID.toString().split(':')[1];
		
	//for page 4m schedule I ,, since there is 2 'I' in ID
	var addToIndex = (id.indexOf("Pg4mScI")>-1) ? 2 : 1;
	//get : 'txtPg10Sc11AI' 
	var untillCharI = (id.indexOf("I"))+addToIndex;
	
	//get letterIndex after "I"
	var letterIndex = -1;
	var afterI = id.substring(untillCharI, id.length);
	
	for (var z=0; z < afterI.length; z++) {
		if (isNaN( afterI.charAt(z))){
			letterIndex = z;
			z =  afterI.length;
		}
	}	

	//get : 'TaxpayerEX1' in'txtPg10Sc11AI59TaxpayerEX1' 
	return  (id.substring((untillCharI+letterIndex), id.length));
}

