function disableEnableRadioButtonByName(mode, name, isCheck) {

	var names = name.split(',');
	
	for (var i = 0; i < names.length; i++) {
		var radioButtons = d.getElementsByName(names[i]);

		for (var j = 0; j < radioButtons.length; j++) {
			
			if (isCheck.toString() != '') {
				radioButtons[j].checked = isCheck;
			}

			radioButtons[j].disabled = (mode == 'disable') ? true : false;               
		}
	}
}

function disableEnableTextbox(mode, controlID, isReset) {

	isReset = (typeof (isReset) === "undefined") ? true : isReset;

	var controlIDs = controlID.split(',');

	for (var i = 0; i < controlIDs.length; i++) {
		d.getElementById("frm1701:" + controlIDs[i]).disabled = (mode == 'disable') ? true : false;
		
		if (isReset) {
			d.getElementById("frm1701:" + controlIDs[i]).value = (d.getElementById("frm1701:" + controlIDs[i]).maxLength == 12) ? "0" : "";
		}
	}
}

