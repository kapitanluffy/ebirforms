//update modal counter
function updateAttachemntModalCounter(id, count){
	
	//collection : EX1:3,EX2,4;SP1:2,
	//same modal, different TYPES
	var collection = d.getElementById(id).value.split(",");
	
	var addCurrent = true;
	var doUpdate = false;
	
	//newly saved modal
	if (collection.length === 0){
		addCurrent = true;
	
	}else{
		
		//check if TYPE is already existing
		//set addCurrent to false if there is match found
		for (var i = 0; i < collection.length; i++){
		
			if (  collection[i].toString().trim() !== "" && collection[i].toString().split(":")[0].toString() === TYPE.toString()  ){
			
				addCurrent = false;
				doUpdate = true;
				collection[i] = TYPE + ":" + count ;
			}
		} 
	}
	
	//just add
	if (addCurrent){
		d.getElementById(id).value += ( TYPE + ":" + count ) + ",";
	}
	//update 
	if (doUpdate){
	
		//reset value
		d.getElementById(id).value = "";
		
		for (var i = 0; i < collection.length; i++){		
			//add updated value
			if (collection[i].toString().trim() !== ""){
				d.getElementById(id).value += collection[i] + ",";
			}
		}
	}
}

//update attachments modal counter
function deleteAttachemntModalCounter(type){

	var counters = d.getElementById("attachmentModalRowCounter").getElementsByTagName("input");
	
	for (var i = 0 ; i < counters.length; i++){
		
		//values of each textbox // EX1:3,EX2:1,SP2:1
		var typeCount = counters[i].value.toString().split(",");
		
		for (var x = 0 ; x < typeCount.length; x++){
		
			if (typeCount[x].split(":")[0] == type){
			
				//just update, set count to zero
				updateAttachemntModalCounter(counters[i].id,0);
			}
		}
	}
}

