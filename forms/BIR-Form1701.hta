    function computeTaxRate(incomeID, taxRateID, outputToID) {
        var taxRateIDValue = convertToFloat(removeCommaParenthesis(taxRateID) / 100);
        var taxRate = 0;
        taxRate = Math.round(convertToFloat(removeCommaParenthesis(incomeID) * taxRateIDValue));

        d.getElementById(outputToID).value = NegativeValue(formatCurrencyWOC(taxRate));
    }

    function computeTaxDueRegular(from, to) {
        var totalTaxableIncome = Math.round(removeCommaParenthesis(from));
        var taxDue = 0;
        var taxableIncome = 0;

        //Not over 10,000
        if (totalTaxableIncome <= 10000 && totalTaxableIncome >= 0) {
            taxDue = totalTaxableIncome * 0.05;
        }
        //Over 10,000 but not over 30,000
        else if (totalTaxableIncome > 10000 && totalTaxableIncome <= 30000) {
            taxableIncome = totalTaxableIncome - 10000;
            taxDue = (taxableIncome * 0.1) + 500;
        }
        //Over 30,000 but not over 70,000
        else if (totalTaxableIncome > 30000 && totalTaxableIncome <= 70000) {
            taxableIncome = totalTaxableIncome - 30000;
            taxDue = (taxableIncome * 0.15) + 2500;
        }
        //Over 70,000 but not over 140,000
        else if (totalTaxableIncome > 70000 && totalTaxableIncome <= 140000) {
            taxableIncome = totalTaxableIncome - 70000;
            taxDue = (taxableIncome * 0.2) + 8500;
        }
        //Over 140,000 but not over 250,000
        else if (totalTaxableIncome > 140000 && totalTaxableIncome <= 250000) {
            taxableIncome = totalTaxableIncome - 140000;
            taxDue = (taxableIncome * 0.25) + 22500;
        }
        //Over 250,000 but not over 500,000
        else if (totalTaxableIncome > 250000 && totalTaxableIncome <= 500000) {
            taxableIncome = totalTaxableIncome - 250000;
            taxDue = (taxableIncome * 0.3) + 50000;
        }
        //Over 500,000
        else if (totalTaxableIncome > 500000) {
            taxableIncome = totalTaxableIncome - 500000;
            taxDue = (taxableIncome * 0.32) + 125000;
        }

        d.getElementById(to).value = NegativeValue(formatCurrencyWOC(Math.round(taxDue)));
    }

    function applicableTaxRatePerTaxTable(value) {
        var totalTaxableIncome = parseFloat(value);
        var taxRate = 0;

        //Not over 10,000
        if (totalTaxableIncome > 0 && totalTaxableIncome <= 10000) {
            taxRate = 5;
        }
        //Over 10,000 but not over 30,000
        else if (totalTaxableIncome > 10000 && totalTaxableIncome <= 30000) {
            taxRate = 10;
        }
        //Over 30,000 but not over 70,000
        else if (totalTaxableIncome > 30000 && totalTaxableIncome <= 70000) {
            taxRate = 15;
        }
        //Over 70,000 but not over 140,000
        else if (totalTaxableIncome > 70000 && totalTaxableIncome <= 140000) {
            taxRate = 20;
        }
        //Over 140,000 but not over 250,000
        else if (totalTaxableIncome > 140000 && totalTaxableIncome <= 250000) {
            taxRate = 25;
        }
        //Over 250,000 but not over 500,000
        else if (totalTaxableIncome > 250000 && totalTaxableIncome <= 500000) {
            taxRate = 30;
        }
        //Over 500,000
        else if (totalTaxableIncome > 500000) {
            taxRate = 32;
        }

        return taxRate;
    }

