    function checkFilingYear() {
        var txtYearEnd = d.getElementById("frm1701:txtPg1I1Year");
        var ddlDate = d.getElementById('frm1701:ddlPg1I1Month');
        var currentDate = new Date();

        var isAmendedReturn = d.getElementById("frm1701:rdoPg1I2AmmendYes").checked; // If Amended Return Yes option is selected, set to true else false
        var isShortPeriod = d.getElementById("frm1701:rdoPg1I3ShortPeriodYes").checked; // If Short Period Return Yes option is selected, set to true else false

        // CALENDAR YEAR
        // If Short Period Return is No and Amended Return is No for 1701,1702 EX,MX and RT
        // then default to December + <Current Year - 1>
        // make non-editable or have validation to not proceed if not met
        if (!isAmendedReturn && !isShortPeriod) {
            //ddlDate.options[12].selected = true;
            ddlDate.value = '12';
            ddlDate.disabled = true;

            txtYearEnd.value = (currentDate.getFullYear() - 2000) - 1;
           // txtYearEnd.disabled = true;
        }
        // If Short Reriod Return is No and Amended Return is Yes for 1701,1702 EX,MX and RT
        // then default to December + editable year field with restriction rules
        else if (!isShortPeriod && !!isAmendedReturn) {
            //ddlDate.options[12].selected = true;
            ddlDate.value = '12';
            ddlDate.disabled = true;

            txtYearEnd.disabled = false;
            //txtYearEnd.value = "";
        }
        // If Short Period Return is Yes and Amended Return is No for 1701,1702 EX,MX and RT
        // then default to editable month field + <Current Year - 1>
        else if (!!isShortPeriod && !isAmendedReturn) {
            ddlDate.disabled = false;
            //ddlDate.options[0].selected = true;
            ddlDate.value = '00';
            ddlDate.focus();

            txtYearEnd.value = (currentDate.getFullYear() - 2000) - 1;
            //txtYearEnd.disabled = true;
        }
        // If Short Period Return is Yes and Amended Return is Yes
        // then default to editable month field + editable year field with restriction rules
        else if (!!isShortPeriod && !!isAmendedReturn) {
            ddlDate.disabled = false;
            //ddlDate.options[0].selected = true;
            ddlDate.value = '00';
            ddlDate.focus();

            txtYearEnd.disabled = false;
            //txtYearEnd.value = "";
        }
    }

