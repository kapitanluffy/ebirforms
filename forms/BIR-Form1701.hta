function sumTaxSpouseIterate() {

	var loopCount = (d.getElementById('tblGrossCompensationAndTaxWithheldRepository').rows.length) + 5;
	var totalTaxPayer = 0;
	var totalSpouse = 0;
	var totalTaxWithheldTaxPayer = 0;
	var totalTaxWithheldSpouse = 0;

	var radioArray = new Array;
	var radioArray1 =  d.getElementById("Page5Content").getElementsByTagName("input");
	var radioArray2 = d.getElementById("tblGrossCompensationAndTaxWithheldRepository").getElementsByTagName("input");
	
	for (a1 = 0; a1 < radioArray1.length; a1++) {
		radioArray.push(radioArray1[a1]);
	}
	for (a2 = 0; a2 < radioArray2.length; a2++) {
		radioArray.push(radioArray2[a2]);
	}

	for (i = 0; i < radioArray.length; i++) {

		if (radioArray[i].type == 'radio') {
			var radioID = radioArray[i].id;

			for (a = 1; a < loopCount; a++) {
		
		//item no 4 not included
		if (d.getElementById("tblGrossCompensationAndTaxWithheldRepository").rows.length >= 1) {
			
			
			if (radioID == "frm1701:rdoPg5Sc1I" + [a] + "Taxpayer" && "frm1701:txtPg5Sc1I" + [a] + "ComIncomeC2" != "frm1701:txtPg5Sc1I4ComIncomeC2") {
				if (d.getElementById("frm1701:rdoPg5Sc1I" + [a] + "Taxpayer").checked) {
				totalTaxPayer += parseInt(removeCommaParenthesis("frm1701:txtPg5Sc1I" + [a] + "ComIncomeC2"));
