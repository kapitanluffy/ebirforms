<script type="text/javascript" name="attachmentIteration">

var TYPE = "";
var PERSON = "";
var defaultPage = 1;

var totalEXArray = [];
var totalSPArray = [];
var currentAttachmentPage = "";
var previousAttachmentPage = "";
var defaultViewPage = "";

var totalEXbyPerson = {};
var totalSPbyPerson = {};

function isAttachmentColumnEmpty(person, attachmentType){
	
	var isColumnEmpty = true;
	
	if (totalEXArray.length <= 1 || totalSPArray.length <=1){
	
		var type = (attachmentType=="EX") ? "EX1" : "SP1";
		var col = (person=="TF") ? "C1" : "C2";
		
		var descriptionID = [];
		descriptionID.push("frm1701:txtPg1mScAI1InvestmentPromotion"+col+type);
		descriptionID.push("frm1701:txtPg1mScAI2LegalBasis"+col+type);
		descriptionID.push("frm1701:txtPg1mScAI3RegisteredActivity"+col+type);
		descriptionID.push("frm1701:txtPg1mScAI5EffectivityDateMonth"+col+type);
		descriptionID.push("frm1701:txtPg1mScAI6EffectivityDateMonth"+col+type);
		
		//validate description if empty string
		for(var i =0; i<descriptionID.length;i++){
			
			if (d.getElementById(descriptionID[i]) != null){
				if(d.getElementById(descriptionID[i]).value.toString().trim() != ""){
					isColumnEmpty = false
				}
			}
		}
		
		var specialTaxRateID = "frm1701:txtPg1mScAI4ISpecialTaxRate"+col+type;
		//validate specialTaxRate if '0.0'
		if (d.getElementById(specialTaxRateID) != null){
			if(d.getElementById(specialTaxRateID).type=="text" && d.getElementById(specialTaxRateID).value.toString().trim() != "0.0"){
				isColumnEmpty = false
			}
		}
	
	}else{
		isColumnEmpty = false;
	}
	
	return isColumnEmpty;
}

function verifyMethodOfDeduction(isExempt){
	
	var doAddAttachment = true;
	
	var onPage1Taxfiler = (d.getElementById("frm1701:txtCurrentPage").value == 1) ? true : false;
	
	var person = (onPage1Taxfiler) ? "Taxfiler" : "Spouse" ;
	
	//Method of Deduction RadioButtons [0]:Itemized [1]:OSD
	var	MOD = [];
		MOD = (onPage1Taxfiler) ? d.getElementsByName("frm1701:rdoPg1Pt1I20Deduction") : d.getElementsByName("frm1701:rdoPg4Pt7I105Itemized")  ;
	var 	isOSD = (MOD[1].checked) ? true : false
	
	if (isOSD === true){
	
		if (!!confirm("Selecting 'OKAY' will automatically change your Method of Deduction to Itemized Deduction. Select 'OKAY' to continue or 'CANCEL' to cancel.")) {
						
			//change MOD to Itemized
			MOD[0].checked = true;
			MOD[1].checked = false;
			
			//ALLOW adding of attachment
			doAddAttachment = true;
			
			//call method of deduction triggers
			methodOfDeductionsTriggers(person);
			
		} else {
			
			var 	rdoAttachmentTrigger = [];
			
			if (onPage1Taxfiler){
			
				rdoAttachmentTrigger = (isExempt) ? d.getElementsByName("frm1701:rdoPg1Pt1I22IncomeExempt") : d.getElementsByName("frm1701:rdoPg1Pt1I23IncomeSpecial")  ;
			}
			else{
			
				rdoAttachmentTrigger = (isExempt) ? d.getElementsByName("frm1701:rdoPg4Pt7I107YesNo") : d.getElementsByName("frm1701:rdoPg4Pt7I108YesNo")  ;
			}
			
			//check NO in adding of attachment
			rdoAttachmentTrigger[0].checked = false;
			rdoAttachmentTrigger[1].checked = true;
			
			//DO NOT ALLOW adding of attachment
			doAddAttachment = false;
		}

	}

	return doAddAttachment;
}
	
function addModals(type) {

	//get source 
	var source = d.getElementById("attachmentModalsSource").innerHTML;

	TYPE = type;

	//put all copied modals depending on what type
	var container = (type.substring(0, 2) === "EX") ? ($("#attachmentModalEX")) : ($("#attachmentModalSP"))

	//replace ID
	var modal = source.replace(/TYPE/g, TYPE);

	//copy modal to container
	container.html(container.html() + modal);
}

function setIterationType(t) { TYPE = t; }

function closeAttachmentModal(e,modalID, isAdd) {

	//=========================================================
	//HIDE MODAL
	//=========================================================
	$('#'+modalID).hide('fade');

	//show background
	//if txtCurrentPage is disabled, show ATTACHMENT
	if (d.getElementById('frm1701:txtCurrentPage').disabled){

		d.getElementById('ATTACHMENT').style.display = "block";
		d.getElementById('PageFooter').style.display = "block";
	}
	else{
		d.getElementById('formPaper').style.display = 'block';
	}

	//=========================================================
	//additional process if user clicked 'CLOSE' when
	//'ADD Attachment' modal popped up
	//=========================================================
	if (e != null && e.id !== "frm1701:btnViewClose"){

		//determine what radiobutton
		//see current page to determine who triggered add attachment
		//page 1 (TAXFILER) or page 2 (SPOUSE)
		var person = (d.getElementById('frm1701:txtCurrentPage').value == "1") ? "TF" : "S";
		//see if EXEMPT, if not, then it is SPECIAL/RATE attachment
		var isExempt = (e.id === "frm1701:btnEXClose") ? true : false;
		var radioButtonName = "";
		
		// if TAXFILER triggered add attachment
		if (person == "TF"){
			
			radioButtonName = (isExempt) ? "frm1701:rdoPg1Pt1I22IncomeExempt" : "frm1701:rdoPg1Pt1I23IncomeSpecial";
		// if SPOUSE triggered add attachment
		}else{
		
			radioButtonName = (isExempt) ? "frm1701:rdoPg4Pt7I107YesNo" : "frm1701:rdoPg4Pt7I108YesNo";
		}

		//get radiobuttons
		//its either [exempt/special] - [ofTaxfiler/ofSpouse] condition above handles it.
		var opt = d.getElementsByName(radioButtonName);
		
		//get total from global variables totalEXbyPerson[] && totalSPbyPerson[]
		var total = (isExempt) ?  totalEXbyPerson[person] : totalSPbyPerson[person];
		//totalEXbyPerson[] or totalSPbyPerson[] may return [undefined/null] value
		//catch [null] value by setting it to zero
		total = (total == null) ? 0 : total;

		//on close & there is no attachment added
		//remove check on [YES] radio button
		if (  (isAdd===false)&&(total==0 )  ){
			
			opt[0].checked = false;
			// to activate checkbox
			document.onmouseup();
		}
	}
}

function verifyActionForAttachment(type){
	
	//this will just close the modal
	//==========================================================
	var emptyObject = null;
	closeAttachmentModal(emptyObject,"attachmentModalADD",true);
	//==========================================================
	
	var total = (type=== "EX") ? (totalEXArray.length) : (totalSPArray.length);

	if (total === 0){

		//addAttachment(type);
		window.setTimeout("addAttachment('"+type+"')", 300);
		
	}else {
	
		showAttachments(true, type);
		displayAttachmentByNumber();

		disablePage5mSchedJItem6();
		//03/13/2014 added rule on Page1m ScheduleA Item4
		//if EXEMPT, disable Schedule A.Item 4A, Item 4B
		if (d.getElementById('frm1701:rdoPg1mExempt' + TYPE).checked) {
		    d.getElementById('frm1701:txtPg1mScAI4ISpecialTaxRateC1' + TYPE).value = "0.0";
		    d.getElementById('frm1701:txtPg1mScAI4ISpecialTaxRateC1' + TYPE).disabled = true;
		    d.getElementById('frm1701:txtPg1mScAI4ISpecialTaxRateC2' + TYPE).value = "0.0";
		    d.getElementById('frm1701:txtPg1mScAI4ISpecialTaxRateC2' + TYPE).disabled = true;
		}
	}
}
	
function verifyShowAttachmentByPerson(type){
	
	//this will just close the modal
	//==========================================================
	var emptyObject = null;
	closeAttachmentModal(emptyObject,'attachmentModalVIEW',false);
	//==========================================================
	
	var p = "";
	
	if (type==="EX"){
		p = (d.getElementById('frm1701:rdoEXAttachmentTF').checked) ? "TF" : "S";
	}else{
		p = (d.getElementById('frm1701:rdoSPAttachmentTF').checked) ? "TF" : "S";
	}

	PERSON = p;
		
	showAttachments(true, type);
	displayAttachmentByNumber();

	disablePage5mSchedJItem6();
	//03/13/2014 added rule on Page1m ScheduleA Item4
	//if EXEMPT, disable Schedule A.Item 4A, Item 4B
	if (d.getElementById('frm1701:rdoPg1mExempt' + TYPE).checked) {
	    d.getElementById('frm1701:txtPg1mScAI4ISpecialTaxRateC1' + TYPE).value = "0.0";
	    d.getElementById('frm1701:txtPg1mScAI4ISpecialTaxRateC1' + TYPE).disabled = true;
	    d.getElementById('frm1701:txtPg1mScAI4ISpecialTaxRateC2' + TYPE).value = "0.0";
	    d.getElementById('frm1701:txtPg1mScAI4ISpecialTaxRateC2' + TYPE).disabled = true;
	}
}
	
//trigger this upon checking radioButtons
//check if radio button is YES or NO
//check if Exempt or Special
//check is Spouse or TAXFILER
function verifyAddAttachment(bool, e, isExempt, person) {
	
	var total = (isExempt) ?  (totalEXArray.length) : (totalSPArray.length);
	PERSON = (person==="SPOUSE") ? "S"  : "TF";
		
	//if radio button [YES] is selected 
	if (bool==true){
		
		var doAdd = false;
		
		//if bool === -1 , do not call verifyMethodOfDeduction()
		//verifyMethodOfDeduction() shows alert
		if (bool === true){
		
			doAdd = verifyMethodOfDeduction(isExempt);
		}
			if (doAdd){
			
				//setup EX header buttons
				if (isExempt) {
				
					d.getElementById('frm1701:btnEXAdd').style.display = "inline";
