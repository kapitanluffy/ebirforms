	moveValue('frm1701:txtPg5Sc1I5ATotalComIncome', 'frm1701:txtPg2Pt4I41GrossCompensationC1');
	moveValue('frm1701:txtPg5Sc1I5BTotalComIncome', 'frm1701:txtPg2Pt4I41GrossCompensationC2');
