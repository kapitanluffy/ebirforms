function addRowPg5Sc1() {

	//call the function needed
	var layoutCell = [];
	layoutCell = schedule1I4ModalLayout();

	//layoutCell[0] is always tempTableID
	//add new rows to tempTable
	var tableTemp = d.getElementById(layoutCell[0].toString());

	//***********************************************************//
	var span = d.getElementById('tblGrossCompensationAndTaxWithheld').getElementsByTagName('span');
	//store valid span here,, is span is format(number.number);
	var validSpan = [];
	var count = 0;
	//validate if span is format(number.number);
	for (var i = 0; i < (span.length); i++) {

		var iterationNumber = span[i].innerHTML.split('.');

		if (iterationNumber.length == 2) {

			if ((!isNaN(iterationNumber[0])) && (!isNaN(iterationNumber[1]))) {
				validSpan[count] = span[i];
				count++;
			}
		}
	};


	var baseNumber = 0;
	
	if (validSpan.length > 0) {
		baseNumber = validSpan[0].innerHTML.split('.')[0];
	}
	//**********************************************************//

	var isValid = validateModalPg5Sc1(tableTemp.id, baseNumber);

	//insert row to tempTable
	if (isValid || isReload) {
		var row = tableTemp.insertRow(tableTemp.rows.length);

		var cell = [];
		cell[0] = row.insertCell(0);
		//start with index 1, index 0 is tableID
		cell[0].innerHTML = layoutCell[1];

		addFocusBlurEvent(tableTemp.id);

		$('input[maxlength="3"][type=text]').keyup(function () {
			var currentValue = $(this).attr('value');
			if (currentValue.length == 3) {
				$(this).closest("input").next().select();
			}
        });
		
		//get radiobuttons 
		var inputs = row.getElementsByTagName("input");
		var rdoCtr=0;
		for (var i = 0; i < (inputs.length); i++) {

			if (inputs[i].type === "radio"){
				
				//refer to item 1 for their state
				//first radioButtonName // taxfiler radiobutton
				if (rdoCtr == 0){
				inputs[i].disabled = d.getElementById('frm1701:rdoPg5Sc1I1Taxpayer').disabled;
				//first radioButtonName // spouse radiobutton
				}else{
				inputs[i].disabled = d.getElementById('frm1701:rdoPg5Sc1I1Spouse').disabled;
				}
				rdoCtr ++;
			}
		}
    }

    //Bug #2582 change the maxlength of Schedule 1 Name of Employer
    if (d.getElementById("frm1701:rdoPg1Pt1I7TypeEstate").checked || d.getElementById("frm1701:rdoPg1Pt1I7TypeTrust").checked) {
        $("#" + tableTemp.id + " input[id*='NameOfEmp']").prop('maxlength', '50');
        $("#" + tableTemp.id + " input[id*='NameOfEmp']").val(($("#" + tableTemp.id + " input[id*='NameOfEmp']").val().length > 50) ? $("#" + tableTemp.id + " input[id*='NameOfEmp']").val().substring(0, 50) : $("#" + tableTemp.id + " input[id*='NameOfEmp']").val());
    } else if (d.getElementById("frm1701:rdoPg1Pt1I7TypeSingle").checked || d.getElementById("frm1701:rdoPg1Pt1I7TypeProfessional").checked) {
        $("#"+ tableTemp.id + " input[id*='NameOfEmp']").prop('maxlength', '90');
    }

	document.onmouseup();
}

