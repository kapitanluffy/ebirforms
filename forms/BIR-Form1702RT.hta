    function checkPg2Pt6I60(elem) {
        var item61 = $_Id('frm1702RT:txtPg2Pt6I61ExpiryDate');


        if (!!validateDate(elem) && ($.trim(elem.value) !== "")) {
            var elemValue = elem.value.split("/");

