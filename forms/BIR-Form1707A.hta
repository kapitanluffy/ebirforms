            var rdo = rdoList[i];
            data = data + "<option value='" + rdo.rdoCode + "'>" + rdo.description + "</option>";
        }
        data = data + "</select>"
        //var data = "<select id='frm1702EX:rdoPg1Pt1I7RDO' name='frm1702EX:rdoPg1Pt1I7RDO' size='1'><option value='000'> </option>";
        $('#rdoContainer').html(data);
