        //num = number.value.toString().replace(/\$|\,/g,'');

        if (amount.indexOf('.') > 0) {
            var parts = amount.toString().split('.');
            parts[0].length;
            if (parts[0].length > 12) {
                return false;
            } else {
                return true;
            }
        } else {
