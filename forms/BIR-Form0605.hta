		var destDir   = "IAF_RDO_Copy";
		fso = new ActiveXObject("Scripting.FileSystemObject");
		if (fso.FolderExists(sourceDir)) {
			  if (fso.FolderExists(drive+destDir)) {
				   //do nothing here...
