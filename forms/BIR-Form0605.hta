		var response = d.getElementById("responseTaxTypeCode");
		
		var taxTypeCodeCnt = String(response.innerHTML).split('taxTypeCodeCount=');
		taxTypeCodeCount = taxTypeCodeCnt[1]; 
	
		var k = 0;
		//loads taxTypeCodeList from xml
		for (var i = 1; i <= taxTypeCodeCount; i++) { 
			
			var taxTypeCode = String(response.innerHTML).split('taxTypeCode'+i+':');
			var taxTypeCodeStr = taxTypeCode[1];
		
			//Ensure that before writing to taxTypeCodePropertyJS the formType 1601F is traceable in taxTypeCodeStr
			if (taxTypeCodeStr.indexOf('0605') >= 0) {
			    var taxTypeCodeValues = taxTypeCodeStr.split('~');							
				var ttypeCode = taxTypeCodeValues[0];
				var description = taxTypeCodeValues[1];				
				
				var objTaxTypeCode = new taxTypeCodePropertyJS(ttypeCode, description, '0605');
				taxTypeCodeList[k] = objTaxTypeCode; 
				k++;
				//alert('0605 successfully created array #'+i);
				
			} else {		
				//alert('0605 not found in array #'+i);
				continue;
			}
		}					
	}	
	
