        if ((d.getElementById('frm1603Q:rdoCode').selectedIndex == 0)) {
            alert("Please enter a valid RDO Code on Item 7.");
            return;
        }
        // if ((d.getElementById('frm1603Q:txtLineBus').value == "")) {
        //     alert("Please enter a valid Line of Business/Occupation on Item 8.");
        //     return;
        // }
        if ((d.getElementById('frm1603Q:txtTaxpayerName').value == "")) {
            alert("Please enter a valid Taxpayer Name on Item 8.");
            return;
        }
        if ((d.getElementById('frm1603Q:txtTelNum').value == "")) {
            alert("Please enter a valid Telephone Number  on Item 10.");
