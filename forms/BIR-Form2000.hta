    function computeSectionATaxDue() {
        var atcBase = parseFloat(d.getElementById('frm2000:ComputeBase').value);
        var atc12b = parseFloat(d.getElementById('frm2000:txt12B').value);
        var atcRate = parseFloat(d.getElementById('frm2000:ComputeRate').value);
        var compType = d.getElementById('frm2000:ComputeCompType').value;
        var constaxDue = parseFloat(d.getElementById('frm2000:ComputeConstantTaxDue').value);
        var minimum =  parseFloat(d.getElementById('frm2000:ComputeMinimum').value);
        var maximum =  parseFloat(d.getElementById('frm2000:ComputeMaximum').value);

        if (compType == "M" ) {
            var tempatc12b =  atc12b - maximum;
            if(tempatc12b < maximum) {
                if(atc12b < minimum ) {
                    //alert("lower in minimum");
                    d.getElementById('frm2000:txt12D').value = "0.00";
                }else {
                    //alert("minimum");
                    d.getElementById('frm2000:txt12D').value = constaxDue.toFixed(2);
                }
            }else { //alert("atc12b " + tempatc12b + " atcBase " + atcBase  + " quotient " + (atc12b / atcBase) )
                var quotienttax = parseInt((tempatc12b / atcBase)  );
                //alert("xyz " + quotienttax);
                if(quotienttax == 1 ) {
                    d.getElementById('frm2000:txt12D').value = constaxDue.toFixed(2);
                }else {
                    d.getElementById('frm2000:txt12D').value = ( (quotienttax * atcRate) + constaxDue*1).toFixed(2);
                }
            }
        } else if( compType == "C"){
            if(atc12b < minimum )
            {
                d.getElementById('frm2000:txt12D').value = (0).toFixed(2);
            }else {
                d.getElementById('frm2000:txt12D').value = constaxDue.toFixed(2);
            }
        } else {
            if(atcBase == 0){
                d.getElementById('frm2000:txt12D').value = (0).toFixed(2);
            }else if( atcBase == 1) {
                
                var taxdue = constaxDue;
                d.getElementById('frm2000:txt12D').value =  ( taxdue*1 ).toFixed(2);
            }else if(atcBase > 0) {
                //alert("wholenum " +  ( atc12b / atcBase).toFixed(0) )
                var wholeNum = ( atc12b / atcBase).toFixed(0);
                var decNum = atc12b - (wholeNum * atcBase);
                if(decNum >= 1) {
                    wholeNum = wholeNum*1 + 1;
                }
                //alert("wholenum " + wholeNum);
                //alert("12D " + (atcRate*1 * wholeNum*1).toFixed(2) );
                d.getElementById('frm2000:txt12D').value = (atcRate*1 * wholeNum*1).toFixed(2);
            }
        }
        d.getElementById('frm2000:txtTax15').value = formatCurrency(NumWithComma(d.getElementById('frm2000:txt12D').value));
        computeAmountOfCompensation();
    }

