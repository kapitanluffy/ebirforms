	createURL = rbURL + "/rest/api/create2?sessionId="+sessionId+"&objName=upload1&tax_due="+p3BasicTax+"&total_penalties="+p3TotalPenalties+"&total_amount_payable="+p3TotalAmountPayable+"&tp_zip="+p3TPZip+"&tp_addr="+p3TPAddress+"&tp_telnum="+p3TPTelNum+"&tp_name="+p3TPName+"&tp_lob="+p3TPLineBus+"&tp_rdo_code="+p3TPRDOCode+"&tp_branch_code="+p3TPBranchCode+"&tp_tin="+p3TPTIN+"&surcharge="+p3Surcharge+"&compromise="+p3Compromise+"&interest="+p3Interest+"&is_amended_return="+p3IsAmended+"&return_period="+p3ReturnPeriod+"&return_period_end="+p3ReturnPeriodEnd+"&useIds=false&form_type_text=2000-Jun2006&load="+xmlFileForCreate2X; //FOR create2
 
	xmlHttp.open( "POST", createURL, false);
	xmlHttp.send();

	var respXMLMsg = xmlHttp.responseText; 

	create2Id = respXMLMsg;
	create2Id = create2Id.split('id=');
	create2Id = String(create2Id[1]).replace(/"/gi,'');
	create2Id = String( parseInt(create2Id) ).replace(/ /,'');
	
	//alert("Form ID : '"+create2Id+"' successfully created!");
	x.document.theForm.submit();
	uploadXMLFile(); 

} 

