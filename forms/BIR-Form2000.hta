    function computeSched1OverallTotal() {
        var summation = 0; 
        for(i=1 ; i <= d.getElementById('tblSched1ATC').rows.length - 2 ; i++ )
        {
            summation+= NumWithComma(d.getElementById('txtTaxDue'+i).value)*1;
        }
        d.getElementById('frm2000:txtSumTaxDue').value =  formatCurrency(summation);
        d.getElementById('frm2000:txt13D').value = formatCurrency(NumWithComma(d.getElementById('frm2000:txtSumTaxDue').value));
        // recompute 13E
        computeAmount13E();
    }
    
