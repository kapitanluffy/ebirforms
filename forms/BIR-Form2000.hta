                    if(result[0] > 12 || result[0] < 0)
                    {
                        alert("Please enter a valid date (mm/dd/yyyy) in Item 13A.");
                        return;
                    }
                }
                if(isNaN(result[1]))
                {
                    alert("Please enter a valid date (mm/dd/yyyy) in Item 13A.");
                    return;
                }else{
                    // numeric check if Date.
                    if(result[1] > 31 || result[1] < 1)
                    {
                        alert("Please enter a valid date (mm/dd/yyyy) in Item 13A.");
                        return;
                    }
                }
                if(isNaN(result[2]))
                {
                    alert("Please enter a valid date (mm/dd/yyyy) in Item 13A.");
                    return;
                }// check year
                if(parseInt(result[2]) > parseInt(d.getElementById('frm2000:txtForYr').value)) {
                    alert("Invalid date entry on Item 13A. Entry should not be later than the return period being filed.");
                    return;
                }
                if(parseInt(result[2]) == parseInt(d.getElementById('frm2000:txtForYr').value) ) { 
                    if( parseInt(result[0],10) > d.getElementById('frm2000:txtMonth1').value*1 ){
                        alert("Invalid date entry on Item 13A. Entry should not be later than the return period being filed.");
                        return;
                    }
                    if( parseInt(result[0],10) == parseInt(d.getElementById('frm2000:txtMonth1').value) ){
                        if( parseInt(result[1]) > parseInt(d.getElementById('frm2000:txtDate').value) ){
                            alert("Invalid date entry on Item 13A. Entry should not be later than the return period being filed.");
                            return;
                        }
                    }
                }
            }else
            {
                alert("Please enter a valid date (mm/dd/yyyy) in Item 13A.");
                return;
            }
        }else {
            alert("Please enter Last Purchase Date in Item 13A."); 
            return;
        }
        
        return true;
    }

    function checkDateSched1()
    {   strtxt13A = d.getElementById('frm2000:txt13A').value;
        var resultTxt13A = strtxt13A.split("/");
        var i;
        var dt = new Date();
        for(i = 1;i <= d.getElementById('tblSched1ATC').rows.length - 2; i++)
        {   var dateFrom = d.getElementById('txtDateFrom'+i).value;
            var dateTo = d.getElementById('txtDateTo'+i).value;
            var txtresultFrom0 = "";
            var txtresultFrom1 = "";
            if(dateFrom != "")
            {
                var result = dateFrom.split("/");
                txtresultFrom0 = result[0];
                txtresultFrom1 = result[1];
                if(result.length == 2 )
                {
                    if(isNaN(result[0]))
                    {   alert("Invalid date entry in Schedule 1, column 1 Record "+i+".Format should be mm/yyyy");
                        return;
                    }else{
                        // numeric check if greater not than 31 - Month.
                        if(result[0] > 12 || result[0] < 0)
                        {
                            alert("Invalid date entry in Schedule 1, column 1 Record "+i+".Format should be mm/yyyy");
                            return;
                        }
                    }
                    if(isNaN(result[1]))
                    {
                        alert("Invalid date entry in Schedule 1, column 1 Record "+i+".Format should be mm/yyyy");
                        return;
                    }else{
                        // numeric check if Year.
                        if(result[1] > dt.getFullYear() || result[1] < 1900)
                        {
                            alert("Invalid date entry in Schedule 1, column 1.Format should be mm/yyyy. Choose within 1900 and not above current Year");
                            return;
                        }
                    } // compate date from
                    if( parseInt(result[1],10) < parseInt(resultTxt13A[2],10)  ){
                        alert("Invalid Inclusive Date of Transaction on Schedule 1 in row 1.\n" +
                            "Entry should be between last purchase date on Item 13A and the return period being filed for on\n" +
                            "Item "+i+"."); return;
                    }
                    if(parseInt(result[1],10) > dt.getFullYear()) {
                        alert("Invalid date entry in Schedule 1 Item no."+i+" in row 1. Entry should not be later than current date."); return;
                    }
                    
                    if(parseInt(result[1],10) == dt.getFullYear() || parseInt(result[1],10) == parseInt(resultTxt13A[2],10)   ){
                        if(parseInt(result[0],10) < parseInt(resultTxt13A[0],10)  ) {
                            alert("Invalid Inclusive Date of Transaction on Schedule 1 in row 1.\n" +
                                "Entry should be between last purchase date on Item 13A and the return period being filed for on\n" +
                                "Item "+i+"."); return;
                        }
                        if(parseInt(result[0],10) > ( dt.getMonth()*1 + 1) && parseInt(result[1],10) > dt.getFullYear()) {
                            alert("Invalid date entry in Schedule 1 Item no.1 in row 1. Entry should not be later than current date."); return;
                        }
                    }

                }else
                {
                    alert("Invalid date entry in Schedule 1, column 1 Record "+i+".Format should be mm/yyyy");
                    return;
                }
            }else {
                alert("Invalid date entry in Schedule 1, column 1 Record "+i+".Format should be mm/yyyy"); return;
            }
            
            if(dateTo != "")
            {
                var result = dateTo.split("/")
                if(result.length == 2 )
                {
                    if(isNaN(result[0]))
                    {   alert("Invalid date entry in Schedule 1, column 2 Record "+i+".Format should be mm/yyyy");
                        return;
                    }else{
                        // numeric check if greater not than 31 - Month.
                        if(result[0] > 12 || result[0] < 0)
                        {
                            alert("Invalid date entry in Schedule 1, column 2 Record "+i+".Format should be mm/yyyy");
                            return;
                        }
                    }
                    if(isNaN(result[1]))
                    {
                        alert("Invalid date entry in Schedule 1, column 2 Record "+i+".Format should be mm/yyyy");
                        return;
                    }else{
                        // numeric check if Year.
                        if(result[1] > dt.getFullYear() || result[1] < 1900)
                        {
                            alert("Invalid date entry in Schedule 1, column 2 Record "+i+".Format should be mm/yyyy. Choose within 1900 and not above current Year");
                            return;
                        }
                    }// compare date to
                    if( parseInt(result[1],10) < parseInt(resultTxt13A[2],10)  ){
                        alert("Invalid Inclusive Date of Transaction on Schedule 1 in row 1.\n" +
                            "Entry should be between last purchase date on Item 13A and the return period being filed for on\n" +
                            "Item "+i+"."); return;
                    }
                    if(parseInt(result[1],10) > dt.getFullYear()) {
                        alert("Invalid date entry in Schedule 1 Item no.2 in row "+i+". Entry should not be later than current date."); return;
                    }

                    // compare from and to
                    var txtStart = new Date(txtresultFrom0 + "/1/" + txtresultFrom1); //alert("" + txtStart);
                    var txtEnd   = new Date(result[0] + "/1/" + result[1] );  //alert("tx " + txtEnd);
                    if( txtStart > txtEnd ){
                        alert("Invalid Inclusive Date of Transaction on Schedule 1 in row "+i+".\n" +
                            "Entry should not be less than corresponding (A) From date and not later than current date."); return;
                    }

                    if(parseInt(result[1],10) == dt.getFullYear() || parseInt(result[1],10) == parseInt(resultTxt13A[2],10)   ){
                        if(parseInt(result[0],10) < parseInt(resultTxt13A[0],10)  ) {
                            alert("Invalid Inclusive Date of Transaction on Schedule 1 in row "+i+".\n" +
                                "Entry should be between last purchase date on Item 13A and the return period being filed for on\n" +
                                "Item 2."); return;
                        }
                        if(parseInt(result[0],10) > ( dt.getMonth()*1 + 1) && parseInt(result[1],10) > dt.getFullYear()) {
                            alert("Invalid date entry in Schedule 1 Item no.2 in row "+i+". Entry should not be later than current date."); return;
                        }
                    }
                    
                }else
                {
                    alert("Invalid date entry in Schedule 1, column 2 Record "+i+".Format should be mm/yyyy");
                    return;
                }
            }else {
                alert("Invalid date entry in Schedule 1, column 2 Record "+i+".Format should be mm/yyyy"); return;
            }
        }
        return true;
    }

