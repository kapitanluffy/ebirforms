	    var titleTin2 = d.getElementById('frm2000:txtTIN2');
		var titleTin3 = d.getElementById('frm2000:txtTIN3');
		var titleBranchCode = d.getElementById('frm2000:txtBranchCode');	
		var titleTPName = d.getElementById('frm2000:txtTaxpayerName');

		var returnPeriod = d.getElementById('frm2000:txtMonth').value + d.getElementById('frm2000:txtYear').value;	
		
		xmlFileName = subFolder + titleTin1.value + titleTin2.value + titleTin3.value + titleBranchCode.value +
		"-2000v2018-" +
		//( (titleTPName.value.replace(/[ ,\^;\)'\(]+/g, '').length > 12) ? titleTPName.value.replace(/[ ,\^;\)'\(]+/g, '').substring(0,12).toLowerCase() : titleTPName.value.replace(/[ ,\^;\)'\(]+/g, '').toLowerCase() ) + 
