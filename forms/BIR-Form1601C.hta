    function getSectionA()
    {
        var size = sectionAToCommit.length;
        sectionA = new Array();
        for(var i=0 ; i < size ; i++)
        {
            sectionA[i] = new SectionA();
            
			if (d.getElementById("frm1601c:SectionA:TxtMonthYear"+i).value == "") {
				alert("Please enter a valid Month/Year.");
				return;
			}
			if (d.getElementById("frm1601c:SectionA:SelMonthYear"+i).value == 'M' && d.getElementById("frm1601c:SectionA:TxtDatePaid"+i).value == "") {
				alert("Please enter a valid Date.");
				return;
			}
			if (d.getElementById("frm1601c:SectionA:SelMonthYear"+i).value == 'M' && d.getElementById("frm1601c:SectionA:TxtRORNo"+i).value == ""){
				alert("Please enter a valid ROR Number.");
				return;
			}
			if (d.getElementById("frm1601c:SectionA:SelMonthYear"+i).value == 'M' && d.getElementById("frm1601c:SectionA:TxtBankCode"+i).value == ""){
				alert("Please enter a valid Bank Code.");
				return;
