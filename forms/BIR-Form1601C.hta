    function computeSectionA()
    {   var taxpaid;
        var shouldTaxDue;
        var tableSize = d.getElementById('tbllistSectionA').rows.length;
        var i;
        var modalSummation = 0;
		
        for (i = 0; i < tableSize ; i++)
        {   
			
			taxpaid =  (d.getElementById('frm1601c:SectionA:TxtTaxPaid'+i).value);
			shouldTaxDue = (d.getElementById('frm1601c:SectionA:TxtShouldTaxDue'+i).value);
			
            if(d.getElementById('frm1601c:SectionA:SelMonthYear'+i).value == "M")
            {
                d.getElementById('frm1601c:SectionA:TxtCurrentYear'+i).value = formatCurrency((NumWithComma(d.getElementById('frm1601c:SectionA:TxtShouldTaxDue'+i).value)*1 - NumWithComma(d.getElementById('frm1601c:SectionA:TxtTaxPaid'+i).value)*1).toFixed(2));
                d.getElementById('frm1601c:SectionA:TxtFromYearEnd'+i).value = "0.00";
            }else{
                d.getElementById('frm1601c:SectionA:TxtFromYearEnd'+i).value = formatCurrency((NumWithComma(d.getElementById('frm1601c:SectionA:TxtShouldTaxDue'+i).value)*1 - NumWithComma(d.getElementById('frm1601c:SectionA:TxtTaxPaid'+i).value)*1).toFixed(2));
                d.getElementById('frm1601c:SectionA:TxtCurrentYear'+i).value = "0.00";
            }
            modalSummation = modalSummation*1 + NumWithComma(d.getElementById('frm1601c:SectionA:TxtFromYearEnd'+i).value)*1 + NumWithComma(d.getElementById('frm1601c:SectionA:TxtCurrentYear'+i).value)*1;
            
        }
        d.getElementById('txtDvTotalAmount7a7b').value = formatCurrency(modalSummation);
		computeSectionARadio();
		d.getElementById('frm1601c:txtTax26').value = formatCurrency(modalSummation + summationTax7a)
		computeOverall26TotalAmount();
		
    }
