								sameFileNameCtr = version+1;
							}
						//}
					}
				}	
				
				if (sameFileNameCtr > 0) {
					newFileNm = subFolder + fileNm + "V" + sameFileNameCtr + ".xml";			
				} else {
					//amended default
					newFileNm = subFolder + fileNm + "V1.xml";
