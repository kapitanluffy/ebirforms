    function processAmend() {
        if (d.getElementById("frm1700:AmendedRtn_1").checked == true) {
            d.getElementById("frm1700:txtPg2Pt4I16C1").disabled = false;
            if (isJointFiling) {
                d.getElementById("frm1700:txtPg2Pt4I16C2").disabled = false;
            }
            d.getElementById("frm1700:txtYear").disabled = false;
            isAmended = true
        }
        else if (d.getElementById("frm1700:AmendedRtn_2").checked == true) {
            d.getElementById("frm1700:txtPg2Pt4I16C1").disabled = true
            d.getElementById("frm1700:txtPg2Pt4I16C2").disabled = true;
            d.getElementById("frm1700:txtPg2Pt4I16C1").value = "0";
            d.getElementById("frm1700:txtPg2Pt4I16C2").value = "0";

            isAmended = false;
            d.getElementById("frm1700:txtYear").disabled = true;
        }
    }

    function processCivilStatus() {

        if (d.getElementById('frm1700:rdoPg1Pt1I13SingleInd').checked) {
            d.getElementById('frm1700:txtPg2Pt4I8C1').value = '50,000';

            processTaxReturn1();
            processTaxReturn2();
        }
        if (d.getElementById('frm1700:rdoPg1Pt1I13MarriedInd').checked) {
            d.getElementById('frm1700:txtPg2Pt4I8C1').value = '50,000';
            if (d.getElementById('frm1700:rdoPg1Pt1I3JointFilingYes').checked == true) {
                d.getElementById('frm1700:txtPg2Pt4I8C2').value = '50,000';
            }
            else {
                d.getElementById('frm1700:txtPg2Pt4I8C2').value = '0';
            }
            processTaxReturn1();
            processTaxReturn2();
        }
        if (d.getElementById('frm1700:rdoPg1Pt1I13LegalSeparatedInd').checked) {
            d.getElementById('frm1700:txtPg2Pt4I8C1').value = '50,000';
            d.getElementById('frm1700:txtPg2Pt4I8C2').value = '0';
            processTaxReturn1();
            processTaxReturn2();
        }
        if (d.getElementById('frm1700:rdoPg1Pt1I13WidowerInd').checked) {
            d.getElementById('frm1700:txtPg2Pt4I8C1').value = '50,000';
            d.getElementById('frm1700:txtPg2Pt4I8C2').value = '0';
            processTaxReturn1();
            processTaxReturn2();
        }
        d.getElementById('frm1700:txtPg2Pt4I8C1').disabled = true;
        d.getElementById('frm1700:txtPg2Pt4I8C2').disabled = true;
    }
    function processJointFiling() {

        if (d.getElementById('frm1700:rdoPg1Pt1I3JointFilingYes').checked == true && d.getElementById('frm1700:rdoPg1Pt1I3JointFilingNo').checked == false) {
            d.getElementById("frm1700:rdoPg1Pt1I13MarriedInd").checked = true;
            isJointFiling = true;
            disableForJointFiling();
            processCivilStatus();
            enableSpouse(true);
        }
        else if (d.getElementById('frm1700:rdoPg1Pt1I3JointFilingYes').checked == false && d.getElementById('frm1700:rdoPg1Pt1I3JointFilingNo').checked == true) {
            //  d.getElementById("frm1700:rdoPg1Pt1I13SingleInd").checked = true;
            isJointFiling = false;
            disableSpouseExemptionClaims();
            processCivilStatus();
            enableSpouse(false);

            resetTfSpEmployers(1);
            resetTfSpEmployers(2);
            resetTfSpEmployers(3);
        }
    }

