        for (var i = 0; i < rdoList.length; i++) {
            var rdo = rdoList[i];
            data = data + "<option value='" + rdo.rdoCode + "'>" + rdo.description + "</option>";
            data2 = data2 + "<option value='" + rdo.rdoCode + "'>" + rdo.description + "</option>";
