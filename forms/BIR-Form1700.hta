    function processClaimAdditionalExemption(person) {

        processPushTempTable2();
        if (person == 'taxpayer') {
            if (d.getElementById('frm1700:rdoPg1Pt1I14YesInd').checked) {

                if (!d.getElementById('frm1700:rdoPg1Pt1I14NoInd').checked && !d.getElementById('frm1700:rdoPg1Pt1I14NoInd').disabled) {

                    d.getElementById('frm1700:txtPg1Pt1I22SpouseQualifiedDependentChildren').value = '';
                    d.getElementById('frm1700:txtPg1Pt1I22SpouseQualifiedDependentChildren').disabled = true;
                    //  d.getElementById('frm1700:txtPg1Pt1I15QualifyDependent').disabled = false;

                    showModalTable2();
                }
                else if (!d.getElementById("frm1700:rdoPg1Pt1I21SpouseClaimAdditionalExemptionYes").disabled && d.getElementById("frm1700:rdoPg1Pt1I21SpouseClaimAdditionalExemptionYes").checked) {

                    d.getElementById('frm1700:txtPg1Pt1I22SpouseQualifiedDependentChildren').value = '';
                    d.getElementById('frm1700:txtPg1Pt1I22SpouseQualifiedDependentChildren').disabled = true;
                    //    d.getElementById('frm1700:txtPg1Pt1I15QualifyDependent').disabled = false;

                    showModalTable2();
                }
            }
            else {
                d.getElementById('frm1700:txtPg1Pt1I22SpouseQualifiedDependentChildren').value = '';
                d.getElementById('frm1700:txtPg1Pt1I15QualifyDependent').value = '';

                ClearTable2();
            }
        }

        if (person == 'spouse') {
            if (d.getElementById('frm1700:rdoPg1Pt1I21SpouseClaimAdditionalExemptionYes').checked) {
                showModalTable2();
            }
            if (d.getElementById('frm1700:rdoPg1Pt1I3JointFilingYes').checked &&
			    d.getElementById('frm1700:rdoPg1Pt1I21SpouseClaimAdditionalExemptionYes').checked &&
				!d.getElementById('frm1700:rdoPg1Pt1I14YesInd').checked && d.getElementById('frm1700:rdoPg1Pt1I14NoInd').checked) {
                d.getElementById('frm1700:rdoPg1Pt1I21SpouseClaimAdditionalExemptionYes').checked = true;
                if (!d.getElementById('frm1700:rdoPg1Pt1I14NoInd').disabled && !d.getElementById('frm1700:rdoPg1Pt1I21SpouseClaimAdditionalExemptionNo').disabled) {

                    d.getElementById('frm1700:txtPg1Pt1I22SpouseQualifiedDependentChildren').value = '';
                    d.getElementById('frm1700:txtPg1Pt1I22SpouseQualifiedDependentChildren').disabled = true;
                    d.getElementById('frm1700:rdoPg1Pt1I21SpouseClaimAdditionalExemptionNo').disabled == false;

                }
            }
            else {
                d.getElementById('frm1700:txtPg1Pt1I22SpouseQualifiedDependentChildren').value = '';
                if (!d.getElementById('frm1700:rdoPg1Pt1I14YesInd').checked) {
                    d.getElementById('frm1700:txtPg1Pt1I15QualifyDependent').value = '';

                    ClearTable2();
                }
            }
        }
        if (d.getElementById('frm1700:rdoPg1Pt1I3JointFilingYes').checked) {
            processTaxReturn1();
            processTaxReturn2();
        }
