        var pWidth = $(window).width();
        var pTop = $(window).scrollTop();
        var eWidth = $(element).width();
        var height = $(element).height();
        $(element).css('top', '130px');
        //$(element).css('top',pTop+100+'px')
        $(element).css('left', parseInt((pWidth / 2) - (eWidth / 2)) + 'px');
