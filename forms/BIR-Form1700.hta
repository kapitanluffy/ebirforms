        else if (d.getElementById("frm1700:rdoPg1Pt1I14NoInd").checked == true) {
            d.getElementById("frm1700:rdoPg1Pt1I14YesInd").checked = false;
            d.getElementById("frm1700:rdoPg1Pt1I14YesInd").disabled = true;
