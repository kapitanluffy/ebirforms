<script type="text/javascript" name="connection to eFPS">

    function resetData() {

        TaxPayerDependentList = [];
        TaxpayerEmployersList = [];
        TaxpayerSpouseEmployersList = [];
        taxpayerCompensationSubjectList = [];
        taxpayerRealPropertySalesList = [];
        taxpayerStockSharesList = [];
        taxpayerOtherIncomeList = [];
        TaxpayerPersonalPropertyReceivedList = [];
        TaxpayerOtherExemptIncomeList = [];
        OtherTaxableIncomeList = [];
    }

    function checkconnection() {
        var status = navigator.onLine;
        if (status) {
            return true;
        } else {
            return false;
        }
    }

    function valueIsString(elementIdWithoutfrm) {

        var stringValue = d.getElementById('frm1700:' + elementIdWithoutfrm).value.toString();

        stringValue = stringValue.replace(/&/g, '&amp;');
        stringValue = stringValue.replace(/</g, '&lt;');
        stringValue = stringValue.replace(/>/g, '&gt;');

        return stringValue;
    }
    function valueIsInt(elementIdWithoutfrm) {

        var intValue = removeCommaParenthesis('frm1700:' + elementIdWithoutfrm);

        return intValue;
    }

    function radioCheckYesNo(element) {

        var status = '';

        if (d.getElementById('frm1700:' + element).checked) {
            status = 'Y'
        } else {
            status = 'N';
        }

        return status;
    }

    function compenOther(element) {
        var status = '';
        if (d.getElementById('frm1700:' + element).checked) {
            status = 'true';
        }
        else {
            status = 'false';
        }

        return status;
    }

    function civilStatusIs(rdo1, rdo2, rdo3, rdo4) {
        var status = '';

        if (d.getElementById('frm1700:' + rdo1).checked) {
            status = "single";
        }
        else if (d.getElementById('frm1700:' + rdo2).checked) {
            status = "married";
        }
        else if (d.getElementById('frm1700:' + rdo3).checked) {
            status = "legally separated";
        }
        else if (d.getElementById('frm1700:' + rdo4).checked) {
            status = "widow/er";
        }

        return status;
    }

    function convertDateXML(element) {
        var date = '';
        var dateSplit = d.getElementById('frm1700:' + element).value.split('/');

        var date = dateSplit[2] + '-' + dateSplit[0] + '-' + dateSplit[1];

        return date;
    }

    function govTypeCTC(r1, r2) {
        var status = '';
        if (d.getElementById('frm1700:' + r1).checked) {
            status = "govtIssuedId";
        } else if (d.getElementById('frm1700:' + r2).checked) {
            status = "ctc";
        }
        return status;
    }

	function submitOnline(){
		if(confirm("You will now be redirected to eFPS Online.")){
			goingToEfps();
		}
	}
	
