    function sumTaxSpouseIterate() {

        var loopCount = (d.getElementById('tblGrossCompensationAndTaxWithheldRepository').rows.length) + 4;
        var totalTaxPayer = 0;
        var totalSpouse = 0;
        var totalTaxWithheldTaxPayer = 0;
        var totalTaxWithheldSpouse = 0;

        var radioArray = new Array;
        var radioArray1 = document.getElementById("Page2Content").getElementsByTagName("input");
        var radioArray2 = document.getElementById("tblGrossCompensationAndTaxWithheldRepository").getElementsByTagName("input");
        // radioArray.push.apply(radioArray, radioArray1);
        //  radioArray.push.apply(radioArray, radioArray2);

        for (a1 = 0; a1 < radioArray1.length; a1++) {
            radioArray.push(radioArray1[a1]);
        }
        for (a2 = 0; a2 < radioArray2.length; a2++) {
            radioArray.push(radioArray2[a2]);
        }

        for (i = 1; i < radioArray.length; i++) {

            if (radioArray[i].type == 'radio' || radioArray[i].type == 'checkbox') {
                var radioID = radioArray[i].id;

                for (a = 1; a < loopCount; a++) {

                    if (d.getElementById("frm1700:txtPg2Pt4I3NameofEmployer").disabled == true) {
                        if (radioID == "frm1700:rdoPg2Pt4I" + [a] + "TaxFiler" && "frm1700:txtPg2Pt4I" + [a] + "ComIncomeC1" != "frm1700:txtPg2Pt4I3ComIncomeC1") {
                            if (d.getElementById("frm1700:rdoPg2Pt4I" + [a] + "TaxFiler").checked) {
                                totalTaxPayer += parseInt(NumWithComma(document.getElementById("frm1700:txtPg2Pt4I" + [a] + "ComIncomeC1").value));
                                totalTaxWithheldTaxPayer += parseInt(NumWithComma(d.getElementById("frm1700:txtPg2Pt4I" + [a] + "TaxWithholdC2").value));
                            }
                        }

                        if (radioID == "frm1700:rdoPg2Pt4I" + [a] + "Spouse" && "frm1700:txtPg2Pt4I" + [a] + "TaxWithholdC2" != "frm1700:txtPg2Pt4I3TaxWithholdC2") {
                            if (d.getElementById("frm1700:rdoPg2Pt4I" + [a] + "Spouse").checked) {
                                totalSpouse += parseInt(NumWithComma(document.getElementById("frm1700:txtPg2Pt4I" + [a] + "ComIncomeC1").value));
                                totalTaxWithheldSpouse += parseInt(NumWithComma(d.getElementById("frm1700:txtPg2Pt4I" + [a] + "TaxWithholdC2").value));
                            }
                        }
                    }

                    else {
                        if (radioID == "frm1700:rdoPg2Pt4I" + [a] + "TaxFiler") {
                            if (d.getElementById("frm1700:rdoPg2Pt4I" + [a] + "TaxFiler").checked) {
                                totalTaxPayer += parseInt(NumWithComma(document.getElementById("frm1700:txtPg2Pt4I" + [a] + "ComIncomeC1").value));
                                totalTaxWithheldTaxPayer += parseInt(NumWithComma(d.getElementById("frm1700:txtPg2Pt4I" + [a] + "TaxWithholdC2").value));
                            }
                        }
                        if (radioID == "frm1700:rdoPg2Pt4I" + [a] + "Spouse") {
                            if (d.getElementById("frm1700:rdoPg2Pt4I" + [a] + "Spouse").checked) {
                                totalSpouse += parseInt(NumWithComma(document.getElementById("frm1700:txtPg2Pt4I" + [a] + "ComIncomeC1").value));
                                totalTaxWithheldSpouse += parseInt(NumWithComma(d.getElementById("frm1700:txtPg2Pt4I" + [a] + "TaxWithholdC2").value));
                            }
                        }
                    }
                }
            }
        }
        //alert('totalTaxPayer = ' + totalTaxPayer);
        d.getElementById("frm1700:txtPg2Pt4I4AC1").value = NegativeValue(formatCurrencyWOC((isNaN(totalTaxPayer) ? 0 : totalTaxPayer)));
        d.getElementById("frm1700:txtPg2Pt4I4AC2").value = NegativeValue(formatCurrencyWOC((isNaN(totalTaxWithheldTaxPayer) ? 0 : totalTaxWithheldTaxPayer)));
        computePt4I15C1();

        d.getElementById("frm1700:txtPg2Pt4I4BC1").value = NegativeValue(formatCurrencyWOC((isNaN(totalSpouse) ? 0 : totalSpouse)));
        d.getElementById("frm1700:txtPg2Pt4I4BC2").value = NegativeValue(formatCurrencyWOC((isNaN(totalTaxWithheldSpouse) ? 0 : totalTaxWithheldSpouse)));
        computePt4I15C2();

        processTaxReturn1();
        processTaxReturn2();
    }
